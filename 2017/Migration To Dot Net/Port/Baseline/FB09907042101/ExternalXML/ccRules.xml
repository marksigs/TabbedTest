<OM_SCHEMA BOOLEANTRUE="1" BOOLEANFALSE="0">
	<ENTITY NAME="APPLICATION" READ_REF="APPLICATION">
		<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="APPLICATION"/>
		<ATTRIBUTE NAME="ISLEGALREPTOBEUSED" ENTITY_REF="APPLICATION"/>
		<ATTRIBUTE NAME="NUMBEROFDEPENDANTS" ENTITY_REF="APPLICATION"/>
		<ENTITY NAME="STAGE" READ_REF="OM_APPLICATIONSTAGE">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="APPLICATIONSTAGE"/>
			<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" ENTITY_REF="APPLICATIONSTAGE" GETDEFAULT="1"/>
			<ATTRIBUTE NAME="STAGESEQUENCENO" ENTITY_REF="APPLICATIONSTAGE"/>
		</ENTITY>
		<ENTITY NAME="APPLICATIONFACTFIND" READ_REF="APPLICATIONFACTFIND">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="APPLICATIONFACTFIND"/>
			<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" ENTITY_REF="APPLICATIONFACTFIND" GETDEFAULT="1"/>
