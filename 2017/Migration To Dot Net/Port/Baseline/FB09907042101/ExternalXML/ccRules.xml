							<ATTRIBUTE NAME="ACCOUNTGUID" ENTITY_REF="ACCOUNT"/>
							<ATTRIBUTE NAME="DIRECTORYGUID" ENTITY_REF="ACCOUNT"/>
							<ATTRIBUTE NAME="THIRDPARTYGUID" ENTITY_REF="ACCOUNT"/>
							<ENTITY NAME="MORTGAGELOAN" READ_REF="MORTGAGELOAN">
								<ATTRIBUTE NAME="ACCOUNTGUID" ENTITY_REF="MORTGAGELOAN" KEYTYPE="FOREIGN"/>
								<ATTRIBUTE NAME="MONTHLYREPAYMENT" ENTITY_REF="MORTGAGELOAN"/>
								<ATTRIBUTE NAME="OUTSTANDINGBALANCE" ENTITY_REF="MORTGAGELOAN"/>
							</ENTITY>
							<ENTITY NAME="MORTGAGEACCOUNT" READ_REF="MORTGAGEACCOUNT">
								<ATTRIBUTE NAME="ACCOUNTGUID" ENTITY_REF="MORTGAGEACCOUNT"/>
								<ATTRIBUTE NAME="REMORTGAGEINDICATOR" ENTITY_REF="MORTGAGEACCOUNT"/>
								<ATTRIBUTE NAME="TOTALCOLLATERALBALANCE" ENTITY_REF="MORTGAGEACCOUNT"/>
								<ATTRIBUTE NAME="TOTALMONTHLYCOST" ENTITY_REF="MORTGAGEACCOUNT"/>
							</ENTITY>
							<ENTITY NAME="LOANSLIABILITIES" READ_REF="LOANSLIABILITIES">
								<ATTRIBUTE NAME="ACCOUNTGUID" ENTITY_REF="LOANSLIABILITIES" KEYTYPE="FOREIGN"/>
								<ATTRIBUTE NAME="AGREEMENTTYPE" ENTITY_REF="LOANSLIABILITIES"/>
								<ATTRIBUTE NAME="MONTHLYREPAYMENT" ENTITY_REF="LOANSLIABILITIES"/>
								<ATTRIBUTE NAME="TOTALOUTSTANDINGBALANCE" ENTITY_REF="LOANSLIABILITIES"/>
							</ENTITY>
						</ENTITY>
					</ENTITY>
					<ENTITY NAME="BANKCREDITCARD" READ_REF="BANKCREDITCARD">
						<ATTRIBUTE NAME="CUSTOMERNUMBER" ENTITY_REF="BANKCREDITCARD"/>
						<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" ENTITY_REF="BANKCREDITCARD"/>
						<ATTRIBUTE NAME="CARDTYPE" ENTITY_REF="BANKCREDITCARD"/>
						<ATTRIBUTE NAME="TOTALOUTSTANDINGBALANCE" ENTITY_REF="BANKCREDITCARD"/>
					</ENTITY>
					<ENTITY NAME="EMPLOYMENT" READ_REF="EMPLOYMENT" >
						<ATTRIBUTE NAME="CUSTOMERNUMBER" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="EMPLOYMENTSEQUENCENUMBER" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="DATESTARTEDORESTABLISHED" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="EMPLOYMENTSTATUS" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="EMPLOYMENTTYPE" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="MAINSTATUS" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="NETMONTHLYINCOME" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="DIRECTORYGUID" ENTITY_REF="EMPLOYMENT"/>
						<ATTRIBUTE NAME="THIRDPARTYGUID" ENTITY_REF="EMPLOYMENT"/>
						<ENTITY ENTITY_REF="EARNEDINCOME"/>
						<!-- EP328 -->
						<ENTITY ENTITY_REF="NETPROFIT"/>
						<ENTITY NAME="NAMEANDADDRESSDIRECTORY" READ_REF="NAMEANDADDRESSDIRECTORY">
							<ATTRIBUTE NAME="DIRECTORYGUID" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
							<ATTRIBUTE NAME="COMPANYNAME" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
							<ATTRIBUTE NAME="ADDRESSGUID" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
							<ATTRIBUTE NAME="CONTACTDETAILSGUID" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
							<ENTITY ENTITY_REF="ADDRESS"/>
							<ENTITY ENTITY_REF="CONTACTDETAILS"/>
							<ENTITY ENTITY_REF="CONTACTTELEPHONEDETAILS"/>
						</ENTITY>
						<ENTITY NAME="THIRDPARTY" READ_REF="THIRDPARTY">
							<ATTRIBUTE NAME="THIRDPARTYGUID" ENTITY_REF="THIRDPARTY"/>
							<ATTRIBUTE NAME="COMPANYNAME" ENTITY_REF="THIRDPARTY"/>
							<ATTRIBUTE NAME="CONTACTDETAILSGUID"  ENTITY_REF="THIRDPARTY"/>
							<ATTRIBUTE NAME="ADDRESSGUID" ENTITY_REF="THIRDPARTY"/>
							<ENTITY ENTITY_REF="ADDRESS"/>
							<ENTITY ENTITY_REF="CONTACTDETAILS"/>
							<ENTITY ENTITY_REF="CONTACTTELEPHONEDETAILS"/>
						</ENTITY>
					</ENTITY>
				</ENTITY>
			</ENTITY>
		</ENTITY>
		<ENTITY ENTITY_REF="GLOBALPARAMETER">
			<ATTRIBUTE NAME="NAME" ENTITY_REF="GLOBALPARAMETER" GETDEFAULT="AddressValidationYears"/>
		</ENTITY>
		<ENTITY ENTITY_REF="GLOBALPARAMETER">
			<ATTRIBUTE NAME="NAME" ENTITY_REF="GLOBALPARAMETER" GETDEFAULT="DCDependantsAtApplicationLevel"/>
		</ENTITY>
	</ENTITY>
</OM_SCHEMA>