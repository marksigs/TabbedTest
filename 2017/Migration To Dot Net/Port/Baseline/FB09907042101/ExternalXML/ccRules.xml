			</ENTITY>
			<ENTITY ENTITY_REF="NEWPROPERTYADDRESS">
				<ENTITY ENTITY_REF="ADDRESS"/>
			</ENTITY>
			<ENTITY ENTITY_REF="NEWPROPERTYROOMTYPE"/>
			<ENTITY ENTITY_REF="NEWPROPERTYVENDOR">
				<ENTITY NAME="THIRDPARTY" READ_REF="THIRDPARTY">
					<ATTRIBUTE NAME="THIRDPARTYGUID" ENTITY_REF="THIRDPARTY"/>
					<ATTRIBUTE NAME="CONTACTDETAILSGUID"  ENTITY_REF="THIRDPARTY"/>
					<ENTITY ENTITY_REF="CONTACTDETAILS"/>
					<ENTITY ENTITY_REF="CONTACTTELEPHONEDETAILS"/>
				</ENTITY>
			</ENTITY>
			<ENTITY ENTITY_REF="OTHERRESIDENT">
				<ENTITY ENTITY_REF="PERSON"/>
			</ENTITY>
			<ENTITY NAME="FEEPAYMENT" READ_REF="FEEPAYMENT">
				<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="FEEPAYMENT"/>
				<ATTRIBUTE NAME="PAYMENTSEQUENCENUMBER" ENTITY_REF="FEEPAYMENT" KEYTYPE="NULL"/>
				<ATTRIBUTE NAME="FEETYPE" DATATYPE="COMBO" COMBOGROUP="OneOffCost" KEYTYPE="NULL"/>
				<ATTRIBUTE NAME="PAYMENTEVENT" DATATYPE="COMBO" COMBOGROUP="PaymentEvent"/>
				<ENTITY NAME="PAYMENTRECORD" READ_REF="PAYMENTRECORD">
					<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="PAYMENTRECORD"/>
					<ATTRIBUTE NAME="PAYMENTSEQUENCENUMBER" ENTITY_REF="PAYMENTRECORD"/>
					<ATTRIBUTE NAME="PAYMENTMETHOD" DATATYPE="COMBO" COMBOGROUP="FeePaymentMethod"/>
				</ENTITY>
			</ENTITY>
			<ENTITY ENTITY_REF="CUSTOMERROLE">
				<ENTITY NAME="CUSTOMERVERSION" READ_REF="CUSTOMERVERSION">
					<ATTRIBUTE NAME="CUSTOMERNUMBER" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="FIRSTFORENAME" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="SURNAME" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="DATEOFBIRTH" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="MARITALSTATUS" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="NUMBEROFDEPENDANTS" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="TIMEATBANKYEARS" ENTITY_REF="CUSTOMERVERSION"/>
					<ATTRIBUTE NAME="TIMEATBANKMONTHS" ENTITY_REF="CUSTOMERVERSION"/>
