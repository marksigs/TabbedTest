			<ENTITY NAME="APPLICATIONBANKBUILDINGSOC" READ_REF="APPLICATIONBANKBUILDINGSOC">
				<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="APPLICATIONBANKBUILDINGSOC"/>
				<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" ENTITY_REF="APPLICATIONBANKBUILDINGSOC"/>
				<ATTRIBUTE NAME="ACCOUNTNAME" ENTITY_REF="APPLICATIONBANKBUILDINGSOC"/>
				<ATTRIBUTE NAME="ACCOUNTNUMBER" ENTITY_REF="APPLICATIONBANKBUILDINGSOC"/>
				<ATTRIBUTE NAME="DIRECTORYGUID" ENTITY_REF="APPLICATIONBANKBUILDINGSOC"/>
				<ATTRIBUTE NAME="THIRDPARTYGUID" ENTITY_REF="APPLICATIONBANKBUILDINGSOC"/>
				<ENTITY NAME="THIRDPARTY" READ_REF="THIRDPARTY">
					<ATTRIBUTE NAME="THIRDPARTYGUID" ENTITY_REF="THIRDPARTY"/>
					<ATTRIBUTE NAME="THIRDPARTYBANKSORTCODE" ENTITY_REF="THIRDPARTY"/>
				</ENTITY>
				<ENTITY NAME="NAMEANDADDRESSDIRECTORY" READ_REF="NAMEANDADDRESSDIRECTORY">
					<ATTRIBUTE NAME="DIRECTORYGUID" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
					<ATTRIBUTE NAME="NAMEANDADDRESSBANKSORTCODE" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
				</ENTITY>
			</ENTITY>
			<ENTITY ENTITY_REF="APPLICATIONLEGALREP">
				<ENTITY ENTITY_REF="CONTACTDETAILS"/>
				<ENTITY ENTITY_REF="CONTACTTELEPHONEDETAILS"/>
				<ENTITY NAME="THIRDPARTY" READ_REF="THIRDPARTY">
					<ATTRIBUTE NAME="THIRDPARTYGUID" ENTITY_REF="THIRDPARTY"/>
					<ATTRIBUTE NAME="COMPANYNAME" ENTITY_REF="THIRDPARTY"/>
					<ATTRIBUTE NAME="CONTACTDETAILSGUID"  ENTITY_REF="THIRDPARTY"/>
					<ATTRIBUTE NAME="ADDRESSGUID" ENTITY_REF="THIRDPARTY"/>
					<ENTITY ENTITY_REF="ADDRESS"/>
					<ENTITY ENTITY_REF="CONTACTDETAILS"/>
					<ENTITY ENTITY_REF="CONTACTTELEPHONEDETAILS"/>
				</ENTITY>
				<ENTITY NAME="NAMEANDADDRESSDIRECTORY" READ_REF="NAMEANDADDRESSDIRECTORY">
					<ATTRIBUTE NAME="DIRECTORYGUID" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
					<ATTRIBUTE NAME="COMPANYNAME" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
					<ATTRIBUTE NAME="CONTACTDETAILSGUID"  ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
					<ATTRIBUTE NAME="ADDRESSGUID" ENTITY_REF="NAMEANDADDRESSDIRECTORY"/>
					<ENTITY ENTITY_REF="ADDRESS"/>
					<ENTITY ENTITY_REF="CONTACTDETAILS"/>
					<ENTITY ENTITY_REF="CONTACTTELEPHONEDETAILS"/>
				</ENTITY>
			</ENTITY>
			<ENTITY NAME="FINANCIALSUMMARY" READ_REF="FINANCIALSUMMARY">
				<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="FINANCIALSUMMARY"/>
				<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" ENTITY_REF="FINANCIALSUMMARY"/>
				<ATTRIBUTE NAME="LOANLIABILITYINDICATOR" ENTITY_REF="FINANCIALSUMMARY"/>
			</ENTITY>
			<ENTITY NAME="NEWPROPERTY" READ_REF="NEWPROPERTY">
				<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="NEWPROPERTY"/>
				<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" ENTITY_REF="NEWPROPERTY"/>
				<ATTRIBUTE NAME="PROPERTYLOCATION" ENTITY_REF="NEWPROPERTY"/>
				<ATTRIBUTE NAME="TYPEOFPROPERTY" ENTITY_REF="NEWPROPERTY"/>
				<ATTRIBUTE NAME="TENURETYPE" ENTITY_REF="NEWPROPERTY"/>
				<ATTRIBUTE NAME="NEWPROPERTYINDICATOR" ENTITY_REF="NEWPROPERTY"/>
				<ATTRIBUTE NAME="HOUSEBUILDERSGUARANTEE" ENTITY_REF="NEWPROPERTY"/>
				<ATTRIBUTE NAME="ANYOTHERRESIDENTSINDICATOR" ENTITY_REF="NEWPROPERTY"/>
