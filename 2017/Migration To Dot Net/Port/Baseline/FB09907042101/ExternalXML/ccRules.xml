					<ENTITY ENTITY_REF="CUSTOMERADDRESS" _ORDERBY="ADDRESSTYPE, DATEMOVEDIN">
						<ENTITY ENTITY_REF="ADDRESS"/>
					</ENTITY>
					<ENTITY ENTITY_REF="CUSTOMERTELEPHONENUMBER"/>
					<ENTITY ENTITY_REF="ACCOUNTRELATIONSHIP">
