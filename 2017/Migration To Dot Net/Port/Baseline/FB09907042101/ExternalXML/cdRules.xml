<?xml version="1.0" encoding="UTF-8"?>
<!-- Used for CD rules -->
<!-- IK 29/06/2006 EP764 EP765 EP766 use omCRUD.xml ENTITY_REF for CUSTOMERADDRESS  -->
<!-- LH 16/02/2007 EP2_1281 -->
<!-- SR 19/03/2007	EP2_1828 : Added INTRODUCERFEE, COMBOGROUP to schema APPLICATION-->
<OM_SCHEMA BOOLEANTRUE="1" BOOLEANFALSE="0">
	<ENTITY NAME="APPLICATION" READ_REF="APPLICATIONFACTFIND">
		<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="APPLICATIONFACTFIND"/>
		<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" ENTITY_REF="APPLICATIONFACTFIND"/>
		<ATTRIBUTE NAME="PURCHASEPRICEORESTIMATEDVALUE" ENTITY_REF="APPLICATIONFACTFIND"/>
		<ATTRIBUTE NAME="NATUREOFLOAN" DATATYPE="COMBO" COMBOGROUP="NatureOfLoan"/>
		<ATTRIBUTE NAME="ACTIVEQUOTENUMBER" ENTITY_REF="APPLICATIONFACTFIND"/>
		<ATTRIBUTE NAME="ACCEPTEDQUOTENUMBER" ENTITY_REF="APPLICATIONFACTFIND"/>
		<ATTRIBUTE NAME="SELFCERTIND" ENTITY_REF="APPLICATIONFACTFIND"/>
		<ENTITY ENTITY_REF="CUSTOMERROLE">
			<ENTITY NAME="CUSTOMER" READ_REF="CUSTOMER" UPDATE_REF="CUSTOMER">
				<ATTRIBUTE NAME="CUSTOMERNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="OTHERSYSTEMCUSTOMERNUMBER" DATATYPE="STRING" LENGTH="12"/>
				<ATTRIBUTE NAME="OTHERSYSTEMTYPE" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="CHANNELID" DATATYPE="STRING" LENGTH="10"/>
				<ATTRIBUTE NAME="INTERMEDIARYGUID" DATATYPE="GUID"/>
			</ENTITY>
			<ENTITY NAME="ALIAS" READ_REF="ALIAS">
				<ATTRIBUTE NAME="CUSTOMERNUMBER" ENTITY_REF="ALIAS"/>
				<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" ENTITY_REF="ALIAS"/>
				<ATTRIBUTE NAME="ALIASSEQUENCENUMBER" ENTITY_REF="ALIAS"/>
				<ATTRIBUTE NAME="ALIASTYPE" DATATYPE="COMBO" COMBOGROUP="AliasType"/>
				<ATTRIBUTE NAME="PERSONGUID" ENTITY_REF="ALIAS"/>
				
				<ENTITY NAME="PERSON" READ_REF="PERSON">
					<ATTRIBUTE NAME="PERSONGUID" ENTITY_REF="PERSON"/>
					<ATTRIBUTE NAME="DATEOFBIRTH" ENTITY_REF="PERSON"/>
					<ATTRIBUTE NAME="FIRSTFORENAME" ENTITY_REF="PERSON"/>
					<ATTRIBUTE NAME="GENDER" ENTITY_REF="PERSON"/>
					<ATTRIBUTE NAME="MARITALSTATUS" ENTITY_REF="PERSON"/>
					<ATTRIBUTE NAME="SECONDFORENAME" ENTITY_REF="PERSON"/>
					<ATTRIBUTE NAME="SURNAME" ENTITY_REF="PERSON"/>
					<ATTRIBUTE NAME="TITLE" DATATYPE="COMBO" COMBOGROUP="Title"/>
					<ATTRIBUTE NAME="TITLEOTHER" ENTITY_REF="PERSON"/>
				</ENTITY>				
			</ENTITY>
			<ENTITY NAME="CUSTOMERVERSION" READ_REF="CUSTOMERVERSION">
				<ATTRIBUTE NAME="CUSTOMERNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" DATATYPE="SHORT" KEYSOURCE="GENERATED" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="DATEOFBIRTH" DATATYPE="DATE"/>
				<ATTRIBUTE NAME="FIRSTFORENAME" DATATYPE="STRING" LENGTH="30"/>
				<ATTRIBUTE NAME="GENDER" DATATYPE="COMBO" COMBOGROUP="Sex"/>
				<ATTRIBUTE NAME="MARITALSTATUS" DATATYPE="COMBO" COMBOGROUP="MaritalStatus"/>
				<ATTRIBUTE NAME="MEMBEROFSTAFF" DATATYPE="BOOLEAN"/>
				<ATTRIBUTE NAME="NATIONALITY" DATATYPE="COMBO" COMBOGROUP="Nationality"/>
				<ATTRIBUTE NAME="OTHERFORENAMES" DATATYPE="STRING" LENGTH="30"/>
				<ATTRIBUTE NAME="SECONDFORENAME" DATATYPE="STRING" LENGTH="30"/>
				<ATTRIBUTE NAME="SURNAME" DATATYPE="STRING" LENGTH="40"/>
				<ATTRIBUTE NAME="TITLE" DATATYPE="COMBO" COMBOGROUP="Title"/>
				<ATTRIBUTE NAME="TITLEOTHER" DATATYPE="STRING" LENGTH="20"/>
				<ATTRIBUTE NAME="NORMALLYRESIDENTINUK" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="UKRESIDENTINDICATOR" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="DEPENDANTINDICATOR" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="NUMBEROFDEPENDANTS" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="CREDITSEARCH" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="TIMEATBANKYEARS" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="TIMEATBANKMONTHS" DATATYPE="SHORT"/>
				<ATTRIBUTE NAME="CONTACTEMAILADDRESS" ENTITY_REF="CUSTOMERVERSION"/>
				
				<ENTITY NAME="EMPLOYMENT" READ_REF="EMPLOYMENT">
					<ATTRIBUTE NAME="CUSTOMERNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
					<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
					<ATTRIBUTE NAME="EMPLOYMENTSEQUENCENUMBER" ENTITY_REF="EMPLOYMENT"/>
					<ATTRIBUTE NAME="DATELEFTORCEASEDTRADING" ENTITY_REF="EMPLOYMENT"/>
					<ATTRIBUTE NAME="DATESTARTEDORESTABLISHED" ENTITY_REF="EMPLOYMENT"/>
					<ATTRIBUTE NAME="EMPLOYMENTSTATUS" ENTITY_REF="EMPLOYMENT"/>
					<ATTRIBUTE NAME="OCCUPATIONTYPE" DATATYPE="COMBO" COMBOGROUP="OccupationType"/>
				</ENTITY>
			</ENTITY>
			<ENTITY ENTITY_REF="CUSTOMERADDRESS">
				<ENTITY NAME="ADDRESS" READ_REF="ADDRESS">
					<ATTRIBUTE NAME="ADDRESSGUID" DATATYPE="GUID" KEYTYPE="FOREIGN" KEYSOURCE="GENERATED"/>
					<ATTRIBUTE NAME="BUILDINGORHOUSENAME" ENTITY_REF="ADDRESS"/>
					<ATTRIBUTE NAME="BUILDINGORHOUSENUMBER" ENTITY_REF="ADDRESS"/>
					<ATTRIBUTE NAME="FLATNUMBER" ENTITY_REF="ADDRESS"/>
					<ATTRIBUTE NAME="STREET" ENTITY_REF="ADDRESS"/>
					<ATTRIBUTE NAME="DISTRICT" ENTITY_REF="ADDRESS"/>
					<ATTRIBUTE NAME="TOWN" ENTITY_REF="ADDRESS"/>
					<ATTRIBUTE NAME="COUNTY" ENTITY_REF="ADDRESS"/>
					<ATTRIBUTE NAME="COUNTRY" DATATYPE="COMBO" COMBOGROUP="Country"/>
					<ATTRIBUTE NAME="POSTCODE" ENTITY_REF="ADDRESS"/>
				</ENTITY>
			</ENTITY>
			<ENTITY NAME="CUSTOMERTELEPHONENUMBER" READ_REF="CUSTOMERTELEPHONENUMBER">
				<ATTRIBUTE NAME="CUSTOMERNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="TELEPHONESEQUENCENUMBER" DATATYPE="SHORT" KEYTYPE="PRIMARY" KEYSOURCE="GENERATED"/>
				<ATTRIBUTE NAME="TELEPHONENUMBER" ENTITY_REF="CUSTOMERTELEPHONENUMBER"/>
				<ATTRIBUTE NAME="USAGE" DATATYPE="COMBO" COMBOGROUP="TelephoneUsage"/>
				<ATTRIBUTE NAME="AREACODE" ENTITY_REF="CUSTOMERTELEPHONENUMBER"/>
			</ENTITY>
			<ENTITY NAME="BANKCREDITCARD" READ_REF="BANKCREDITCARD">
				<ATTRIBUTE NAME="CUSTOMERNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="SEQUENCENUMBER" DATATYPE="SHORT" KEYTYPE="PRIMARY" KEYSOURCE="GENERATED"/>
				<ATTRIBUTE NAME="CARDTYPE" DATATYPE="COMBO" COMBOGROUP="CreditCardType"/>
			</ENTITY>
			<ENTITY NAME="INCOMESUMMARY" READ_REF="INCOMESUMMARY">
				<ATTRIBUTE NAME="CUSTOMERNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="CUSTOMERVERSIONNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="ALLOWABLEANNUALINCOME" ENTITY_REF="INCOMESUMMARY"/>
			</ENTITY>
		</ENTITY>
		<ENTITY NAME="NEWLOAN" READ_REF="NEWLOAN">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="NEWLOAN"/>
			<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" ENTITY_REF="NEWLOAN"/>
			<ATTRIBUTE NAME="AMOUNTREQUESTED" ENTITY_REF="NEWLOAN"/>
		</ENTITY>
		<ENTITY NAME="QUOTATION" READ_REF="QUOTATION">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
			<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
			<ATTRIBUTE NAME="QUOTATIONNUMBER" DATATYPE="SHORT" KEYTYPE="PRIMARY" KEYSOURCE="GENERATED"/>
			<ATTRIBUTE NAME="MORTGAGESUBQUOTENUMBER" ENTITY_REF="QUOTATION"/>
			<ENTITY NAME="MORTGAGESUBQUOTE" READ_REF="MORTGAGESUBQUOTE">
				<ATTRIBUTE NAME="APPLICATIONNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="MORTGAGESUBQUOTENUMBER" DATATYPE="SHORT" KEYTYPE="PRIMARY" KEYSOURCE="GENERATED"/>
				<ATTRIBUTE NAME="AMOUNTREQUESTED" ENTITY_REF="MORTGAGESUBQUOTE"/>
				<ATTRIBUTE NAME="INCOMEMULTIPLE" ENTITY_REF="MORTGAGESUBQUOTE"/>
				<ENTITY NAME="LOANCOMPONENT" READ_REF="LOANCOMPONENT">
					<ATTRIBUTE NAME="APPLICATIONNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
					<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
					<ATTRIBUTE NAME="MORTGAGESUBQUOTENUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
					<ATTRIBUTE NAME="LOANCOMPONENTSEQUENCENUMBER" DATATYPE="SHORT" KEYTYPE="PRIMARY" KEYSOURCE="GENERATED"/>
					<ATTRIBUTE NAME="TERMINMONTHS" ENTITY_REF="LOANCOMPONENT"/>
					<ATTRIBUTE NAME="TERMINYEARS" ENTITY_REF="LOANCOMPONENT"/>
				</ENTITY>
			</ENTITY>
		</ENTITY>
		<ENTITY NAME="NEWPROPERTY" READ_REF="NEWPROPERTY">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
			<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
			<ATTRIBUTE NAME="TYPEOFPROPERTY" ENTITY_REF="NEWPROPERTY"/>
			<ATTRIBUTE NAME="VALUATIONTYPE" DATATYPE="COMBO" COMBOGROUP="ValuationType"/>
			<ENTITY NAME="NEWPROPERTYROOMTYPE" READ_REF="NEWPROPERTYROOMTYPE">
				<ATTRIBUTE NAME="APPLICATIONNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
				<ATTRIBUTE NAME="ROOMTYPE" KEYTYPE="FOREIGN" DATATYPE="COMBO" COMBOGROUP="NewPropertyRoomType"/>
				<ATTRIBUTE NAME="NUMBEROFROOMS" DATATYPE="SHORT"/>
			</ENTITY>
		</ENTITY>
		<ENTITY NAME="OTHERRESIDENT" READ_REF="OTHERRESIDENT">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
			<ATTRIBUTE NAME="APPLICATIONFACTFINDNUMBER" DATATYPE="SHORT" KEYTYPE="FOREIGN"/>
			<ATTRIBUTE NAME="OTHERRESIDENTSEQUENCENUMBER" DATATYPE="SHORT" KEYTYPE="PRIMARY" KEYSOURCE="GENERATED"/>
			<ATTRIBUTE NAME="PERSONGUID" DATATYPE="GUID"/>
		</ENTITY>
		<ENTITY ENTITY_REF="INTRODUCERFEE">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" DATATYPE="STRING" LENGTH="12" KEYTYPE="FOREIGN"/>
		</ENTITY>
		<ENTITY ENTITY_REF="GLOBALPARAMETER">
			<ATTRIBUTE NAME="NAME" ENTITY_REF="GLOBALPARAMETER" GETDEFAULT="TMReprocessCreditCheck"/>
		</ENTITY>
		<ENTITY ENTITY_REF="GLOBALPARAMETER">
			<ATTRIBUTE NAME="NAME" ENTITY_REF="GLOBALPARAMETER" GETDEFAULT="TMRescoreCreditCheck"/>
		</ENTITY>
		<ENTITY ENTITY_REF="GLOBALPARAMETER">
			<ATTRIBUTE NAME="NAME" ENTITY_REF="GLOBALPARAMETER" GETDEFAULT="TMRemodelMortgage"/>
		</ENTITY>
		<ENTITY ENTITY_REF="COMBOGROUP">
			<ATTRIBUTE NAME="GROUPNAME" ENTITY_REF="COMBOGROUP" GETDEFAULT="OneOffCost"/>
		</ENTITY>		
	</ENTITY>
</OM_SCHEMA>
