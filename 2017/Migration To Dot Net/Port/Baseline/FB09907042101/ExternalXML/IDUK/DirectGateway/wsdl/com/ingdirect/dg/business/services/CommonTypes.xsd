	<complexType name="CardPaymentDetailsType" abstract="false">
		<annotation>
			<documentation>Card payment request sent to card vendor</documentation>
		</annotation>
		<sequence>
			<element name="CreditCardDetails" type="common:CreditOrDebitCardDetailsType"/>
			<element name="MasterAccountNumber" nillable="true" minOccurs="0">
				<simpleType>
					<restriction base="int">
						<totalDigits value="8"/>
					</restriction>
				</simpleType>
			</element>
			<element name="MortgageApplicationNumber" type="string" nillable="true" minOccurs="0"/>
			<element name="PaymentMechanism" type="string">
				<annotation>
					<documentation>This value should be set to "CreditCard"</documentation>
				</annotation>
			</element>
			<element name="TransactionMode" type="string">
				<annotation>
					<documentation>P -Production mode. Indicates the transaction is real. 
							N -Sends the transaction to the internal payment simulator. The simulator sends a No value, indicating the transaction was rejected. 
							Y -Sends the transaction to the internal payment simulator. The simulator sends a Yes value, indicating the transaction was accepted. 
							R -Sends the transaction to t	he internal payment simulator. The simulator sends a random Yes or No value. 
			</documentation>
				</annotation>
			</element>
			<element name="TransactionType" type="string" nillable="true">
				<annotation>
					<documentation>This value should be set to "Auth". 
			</documentation>
				</annotation>
			</element>
			<element name="Pipeline" type="string">
				<annotation>
					<documentation>This value should be set to "Payment".
			</documentation>
				</annotation>
			</element>
		</sequence>
