	<complexType name="BankAccountDetailsType" abstract="false">
		<annotation>
			<documentation>Structure for a Payment Account or a Solicitors Bank Account</documentation>
		</annotation>
		<sequence>
			<element name="AccountName" nillable="true" minOccurs="0">
				<annotation>
					<documentation>Name of bank account</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="18"/>
					</restriction>
				</simpleType>
			</element>
			<element name="SortCode">
				<annotation>
					<documentation>The 6 digit sort code for the bank account</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="9"/>
					</restriction>
				</simpleType>
			</element>
			<element name="AccountNumber">
				<annotation>
					<documentation>The account number for either the solicitor or applicant bank account.</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="13"/>
					</restriction>
				</simpleType>
			</element>
			<element name="TransposedIndicator" minOccurs="0">
				<annotation>
					<documentation>Indicates that the bank account number has been altered to comply with BACS/CHAPS file formats.</documentation>
				</annotation>
				<simpleType>
					<restriction base="boolean"/>
				</simpleType>
			</element>
			<element name="RedirectedIndicator" minOccurs="0">
				<simpleType>
					<restriction base="boolean"/>
				</simpleType>
			</element>
			<element name="DDIStatus" nillable="true" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<maxLength value="1"/>
					</restriction>
				</simpleType>
			</element>
			<element name="BankName" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<maxLength value="18"/>
					</restriction>
				</simpleType>
			</element>
			<element name="PaymentDayInMonth" minOccurs="0">
				<simpleType>
					<restriction base="integer"/>
				</simpleType>
			</element>
			<element name="OriginatorIdentificationNumber" minOccurs="0">
				<simpleType>
					<restriction base="string"/>
				</simpleType>
			</element>
			<element name="BankAddress" type="common:StructuredAddressDetailsType" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="StructuredAddressDetailsType" abstract="false">
		<annotation>
			<documentation>Structured address is used for all address capture. For the prospect/customer, structured addresses include Legal, Mailing and Previous addresses. For the mortgage application, structured addresses include the Property Address and Solicitor Address.</documentation>
		</annotation>
		<sequence>
			<element name="FlatNameOrNumber" nillable="true" minOccurs="0">
				<annotation>
					<documentation>Flat name or number of address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="40"/>
					</restriction>
				</simpleType>
			</element>
			<element name="HouseOrBuildingName" nillable="true" minOccurs="0">
				<annotation>
					<documentation>House or building name of address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="30"/>
					</restriction>
				</simpleType>
			</element>
			<element name="HouseOrBuildingNumber" nillable="true" minOccurs="0">
				<annotation>
					<documentation>House number of address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="6"/>
					</restriction>
				</simpleType>
			</element>
			<element name="Street" nillable="true" minOccurs="0">
				<annotation>
					<documentation>Street name of address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="40"/>
					</restriction>
				</simpleType>
			</element>
			<element name="District" nillable="true" minOccurs="0">
				<annotation>
					<documentation>District name of address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="40"/>
					</restriction>
				</simpleType>
			</element>
			<element name="TownOrCity" nillable="true" minOccurs="0">
				<annotation>
					<documentation>Town or City name of address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="40"/>
					</restriction>
				</simpleType>
			</element>
			<element name="County" nillable="true" minOccurs="0">
				<annotation>
					<documentation>County name of address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="40"/>
					</restriction>
				</simpleType>
			</element>
			<element name="PostCode" type="common:PostCodeType" minOccurs="0">
				<annotation>
					<documentation>Post code of address. See PostCodeType for formats</documentation>
				</annotation>
			</element>
			<element name="CountryCode" nillable="true" minOccurs="0">
				<annotation>
					<documentation>Indicates the country for the address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="3"/>
					</restriction>
				</simpleType>
			</element>
			<element name="Overriden" nillable="true" minOccurs="0">
				<annotation>
					<documentation>Indicates that the addresses returned by the PAF search do not include the person's correct address. Allows user to override addrress returned by the PAF search, and enter amended details.</documentation>
				</annotation>
				<simpleType>
					<restriction base="boolean"/>
				</simpleType>
			</element>
			<element name="AddressType" nillable="true" minOccurs="0">
				<annotation>
					<documentation>H = Legal address, P = Previous legal, M = Mail address</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="1"/>
						<enumeration value="H"/>
						<enumeration value="P"/>
						<enumeration value="M"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
	<complexType name="PhoneDetailsType" abstract="false">
		<annotation>
			<documentation>Phone number captured on F/E systems is made up of both "AreaCode" and "LocalNumber" elements.</documentation>
		</annotation>
		<sequence>
			<element name="AreaCode" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<pattern value="[0-9]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="LocalNumber">
				<simpleType>
					<restriction base="string">
						<pattern value="[0-9]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="Extension">
				<simpleType>
					<restriction base="string">
						<pattern value="[0-9]*"/>
					</restriction>
				</simpleType>
			</element>
			<element name="PhoneType">
				<annotation>
					<documentation>C = Cellphone/Mobile, R = Residential, B = Business, U = Unknown, F = Fax</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<maxLength value="1"/>
						<enumeration value="C"/>
						<enumeration value="R"/>
						<enumeration value="B"/>
						<enumeration value="U"/>
						<enumeration value="F"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
	<complexType name="CreditOrDebitCardDetailsType" abstract="false">
		<annotation>
			<documentation>This element holds the details of either credit card or debit card details</documentation>
		</annotation>
		<sequence>
			<element name="CardAccountNumber" nillable="false">
				<simpleType>
					<restriction base="string">
						<maxLength value="19"/>
					</restriction>
				</simpleType>
			</element>
			<element name="CardExpiryDate" type="string" nillable="false"/>
			<element name="CardVerificationNumber" nillable="true" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<maxLength value="4"/>
					</restriction>
				</simpleType>
			</element>
			<element name="CardVerificationIndicator" nillable="false" minOccurs="0">
				<annotation>
					<documentation>1- CVM value present, and submitted from the Store, 5 - CVM is intentionally not provided.</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<pattern value="1"/>
						<pattern value="5"/>
					</restriction>
				</simpleType>
			</element>
			<element name="CardType" nillable="false">
				<annotation>
					<documentation>1 - Visa, 2 - MasterCard, 8 - American Express , 9 - Solo, 10 - Switch, 11 - Electron</documentation>
				</annotation>
				<simpleType>
					<restriction base="string">
						<enumeration value="1"/>
						<enumeration value="2"/>
						<enumeration value="9"/>
						<enumeration value="10"/>
						<enumeration value="11"/>
					</restriction>
				</simpleType>
			</element>
			<element name="CardIssueNumber" nillable="true" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<maxLength value="2"/>
					</restriction>
				</simpleType>
			</element>
			<element name="CardStartDate" type="string" nillable="true" minOccurs="0"/>
			<element name="PaymentAmount" type="decimal" nillable="false"/>
			<element name="Address" type="common:StructuredAddressDetailsType"/>
		</sequence>
	</complexType>
	<complexType name="ConveyancerAcknowledgementType" abstract="false">
		<annotation>
			<documentation>This is response to all request except polling</documentation>
		</annotation>
		<sequence>
			<element name="SequenceNo" type="common:SequenceNoType">
				<annotation>
					<documentation>IDUK Sequence# sent as part of the request.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="ConveyancerHeaderType" abstract="false">
		<sequence>
			<element name="Timestamp" type="common:TimeStampType"/>
		</sequence>
	</complexType>
	<simpleType name="SequenceNoType">
		<annotation>
			<documentation>IDUK Sequence No. for Conveyancing Request</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="\d{1,10}"/>
		</restriction>
	</simpleType>
	<simpleType name="ConveyancerSequenceNoType">
		<annotation>
			<documentation>Conveyancer Sequence No. in the Response</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="\d{1,10}"/>
		</restriction>
	</simpleType>
	<simpleType name="SimpleDateType">
		<annotation>
			<documentation>This data type is used to represent simple date of the format YYYY/MM/DD.</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="\d{4}/(0?[1-9]|1[0-2])/(3[01]|[1-2][0-9]|0?[1-9])"/>
		</restriction>
	</simpleType>
	<simpleType name="TimeStampType">
		<annotation>
			<documentation>This data type is used to represent time stamp of the format YYYY/MM/DD hh:mm:ss</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="\d{4}/(0?[1-9]|1[0-2])/(3[01]|[1-2][0-9]|0?[1-9]) (0?\d|1[0-9]|2[0-3]):(0?\d|[1-5]\d):(0?\d|[1-5]\d)"/>
		</restriction>
	</simpleType>
	<simpleType name="TargetType">
		<restriction base="string">
			<enumeration value="C"/>
			<enumeration value="P"/>
		</restriction>
	</simpleType>
	<simpleType name="ActionType">
		<restriction base="string">
			<enumeration value="INS"/>
			<enumeration value="UPD"/>
			<enumeration value="DEL"/>
		</restriction>
	</simpleType>
	<simpleType name="GenderType">
		<annotation>
			<documentation>M = Male; F = Female; X = Not disclosed</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="M"/>
			<enumeration value="F"/>
			<enumeration value="X"/>
		</restriction>
	</simpleType>
	<simpleType name="ResidentType">
		<restriction base="string">
			<enumeration value="0"/>
			<enumeration value="1"/>
			<enumeration value="2"/>
		</restriction>
	</simpleType>
	<simpleType name="RelationshipCodeType">
		<restriction base="string">
			<enumeration value="A"/>
			<enumeration value="B"/>
			<enumeration value="PA"/>
		</restriction>
	</simpleType>
	<simpleType name="ClientStatusType">
		<annotation>
			<documentation>This is the customer status indicator. Values 3 and 4 below are legacy from Canada DAWT, and refer to business prospect and customers. 1 = Prospect; 2 = Customer</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="1"/>
			<enumeration value="2"/>
			<enumeration value="3"/>
			<enumeration value="4"/>
		</restriction>
	</simpleType>
	<simpleType name="ClientType">
		<restriction base="string">
			<enumeration value="0"/>
		</restriction>
	</simpleType>
	<simpleType name="EmailOptionType">
		<restriction base="string">
			<enumeration value="G"/>
			<enumeration value="H"/>
		</restriction>
	</simpleType>
	<simpleType name="BusinessType">
		<restriction base="string">
			<enumeration value="1"/>
			<enumeration value="2"/>
			<enumeration value="3"/>
			<enumeration value="4"/>
		</restriction>
	</simpleType>
	<simpleType name="SpecialNeedType">
		<annotation>
			<documentation>Refers to the Special Needs fulfilment indicator. 0 = No special needs; 1 = Large Print; 2 = Audio fulfilment; 3 = Braille fulfilment</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="0"/>
			<enumeration value="L"/>
			<enumeration value="A"/>
			<enumeration value="B"/>
		</restriction>
	</simpleType>
	<simpleType name="StatementType">
		<restriction base="string">
			<enumeration value="1"/>
			<enumeration value="2"/>
		</restriction>
	</simpleType>
	<simpleType name="FulfilmentPackIdType">
		<annotation>
			<documentation>defines the GUID type to represent the 128 bit windows guid type as 32 hex digit. This field refers to the unique ID assigned to each fulfilment pack.</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[A-Fa-f0-9]{32}"/>
		</restriction>
	</simpleType>
	<simpleType name="FulfilmentResponseType">
		<annotation>
			<documentation>The response is a generic response indicating the outcome of the request. The general values that can be expected are :"Accepted", "Declined", "Error". In case of error/declined response the error message would be set to appropriate message.                             </documentation>
		</annotation>
		<restriction base="string"/>
	</simpleType>
