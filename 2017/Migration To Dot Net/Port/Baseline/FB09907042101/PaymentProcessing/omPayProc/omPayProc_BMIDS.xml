<!-- PSC 19/08/2002 BMIDS00330 Added TOTALMONTHLYCOST and TOTALACCRUEDINTEREST to DISBURSEMENTPAYMENT -->
<!--                           and ACCRUEDINTEREST to LOANCOMPONENTPAYMENT -->
<!--                           Amended INITIALREPAYMENT and MONTHLYREPAYMENT to be CURRENCY data types in LOANCOMPONENTPAYMENT -->
<!-- PSC 19/08/2002 BMIDS00643 Add in bits that were missing -->
<!-- PSC 13/11/2002 BMIDS00931 Added INTERESTONLYELEMENT and CAPITALANDINTERESTELEMENT to FINDLOANCOMPONENTPAYMENTLIST  -->
<!-- PSC 20/11/2002 BMIDS01031 Added ROFTOTALS -->
<!-- RF  20/11/2002 BMIDS01038 Added SQLNOLOCK="TRUE" to:
				APPLICATIONFEETYPE\FEETYPESEQUENCENUMBER 
				PAYMENTRECORD\PAYMENTSEQUENCENUMBER 
				LOANCOMPONENTPAYMENT\PAYMENTSEQUENCENUMBER 
				PAYEEHISTORY\PAYEEHISTORYSEQNO 
				APPFEEPAY\PAYMENTSEQUENCENUMBER 
				APPPAYREC\PAYMENTSEQUENCENUMBER 
				PAYPROCBATCHAUDIT\PAYMENTSEQUENCENUMBER 
				BATCHPAYMENT\PAYMENTSEQUENCENUMBER -->
<!-- MV  03/03/2003 BM0378 	Added SQLNOLOCK="TRUE" to:APPLICATIONFEETYPE -->
<!-- MDC 10/03/2003 BM0339	Added SQLNOLOCK to DISBURSEMENTPAYMENT -->
<PAYMENTPROCESSING>
   <APPLICATIONFEETYPE ENTITYTYPE="PHYSICAL" SQLROWLOCK="TRUE" DATASRCE="APPLICATIONFEETYPE">
      <FEETYPESEQUENCENUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
   </APPLICATIONFEETYPE>
   <PAYMENTRECORD ENTITYTYPE="PHYSICAL" DATASRCE="PAYMENTRECORD">
      <PAYMENTSEQUENCENUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
   </PAYMENTRECORD>
   <DISBURSEMENTPAYMENT ENTITYTYPE="PHYSICAL" DATASRCE="DISBURSEMENTPAYMENT" SQLNOLOCK="TRUE">
      <TOTALMONTHLYCOST DATATYPE="CURRENCY"/>
      <TOTALACCRUEDINTEREST DATATYPE="CURRENCY"/>
      <SHORTFALL DATATYPE="CURRENCY"/>
   </DISBURSEMENTPAYMENT>
   <LOANCOMPONENTPAYMENT ENTITYTYPE="PHYSICAL" DATASRCE="LOANCOMPONENTPAYMENT">
      <PAYMENTSEQUENCENUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
      <INITIALREPAYMENT DATATYPE="CURRENCY"/>
      <MONTHLYREPAYMENT DATATYPE="CURRENCY"/>
      <ACCRUEDINTEREST DATATYPE="CURRENCY"/>
   </LOANCOMPONENTPAYMENT>
   <APPFEEPAY ENTITYTYPE="PHYSICAL" DATASRCE="FEEPAYMENT" OUTNAME="FEEPAYMENT">
      <PAYMENTSEQUENCENUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
   </APPFEEPAY>
   <APPPAYREC ENTITYTYPE="PHYSICAL" DATASRCE="FEEPAYMENT" OUTNAME="FEEPAYMENT">
      <PAYMENTSEQUENCENUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
   </APPPAYREC>
   <FINDPAYMENTSFORPAYEE ENTITYTYPE="LOGICAL" DATASRCE="FINDPAYMENTSFORPAYEELIST" OUTNAME="DISBURSEMENTPAYMENT">
      <PAYMENTSTATUS DATATYPE="SHORT"/>
      <PAYMENTTYPE DATATYPE="SHORT"/>
      <NETPAYMENTAMOUNT DATATYPE="LONG"/>
   </FINDPAYMENTSFORPAYEE>
   <CHEQUEAUDIT ENTITYTYPE="PHYSICAL" DATASRCE="CHEQUEAUDIT">
      <APPLICATIONNUMBER DATATYPE="STRING" KEYTYPE="PRIMARY" LENGTH="12"/>
      <PAYMENTSEQUENCENUMBER DATATYPE="SHORT" KEYTYPE="PRIMARY"/>
      <CACREATIONDATETIME DATATYPE="DATETIME"/>
      <CACHEQUENUMBER DATATYPE="STRING" LENGTH="20"/>
      <CAUSERID DATATYPE="STRING" LENGTH="10"/>
   </CHEQUEAUDIT>
   <PAYPROCBATCHAUDIT ENTITYTYPE="PHYSICAL" DATASRCE="PAYPROCBATCHAUDIT" SQLNOLOCK="TRUE">
      <PAYMENTTYPE DATATYPE="COMBO" COMBOGROUP="PaymentType"/>
      <PAYMENTSEQUENCENUMBER KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
   </PAYPROCBATCHAUDIT>
   <INTERFACEDINITIALADVANCE ENTITYTYPE="LOGICAL" DATASRCE="INTERFACEDINITIALADVANCES">
      <APPLICATIONNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="12"/>
      <PAYMENTSEQUENCENUMBER KEYTYPE="PRIMARY" DATATYPE="SHORT"/>
      <PAYMENTSTATUS DATATYPE="COMBO" COMBOGROUP="PaymentStatus"/>
      <PROCESSINGDATE DATATYPE="DATE"/>
      <COMPLETIONDATE DATATYPE="DATE"/>
      <CANCELLATIONDATE DATATYPE="DATE"/>
      <MANUALPAYMENTINDICATOR DATATYPE="BOOLEAN"/>
      <TTREFNUMBER1 DATATYPE="STRING" LENGTH="7"/>
      <TTREFNUMBER2 DATATYPE="STRING" LENGTH="9"/>
      <PAYMENTNOTES DATATYPE="STRING" LENGTH="255"/>
      <ISSUEDATE DATATYPE="DATE"/>
      <PAYMENTTYPE DATATYPE="COMBO" COMBOGROUP="PaymentType"/>
      <PAYEEHISTORYSEQNO DATATYPE="SHORT"/>
      <PAYEETYPE DATATYPE="COMBO" COMBOGROUP="PayeeType"/>
      <NETPAYMENTAMOUNT DATATYPE="LONG"/>
      <TOTALMONTHLYCOST DATATYPE="CURRENCY" OOSS="VARIANT"/>
      <TOTALACCRUEDINTEREST DATATYPE="CURRENCY" OOSS="VARIANT"/>
      <SHORTFALL DATATYPE="CURRENCY" OOSS="VARIANT"/>
   </INTERFACEDINITIALADVANCE>
   <FEEPAYMENT ENTITYTYPE="PHYSICAL" DATASRCE="FEEPAYMENT">
      <COMPLETIONINDICATOR OOSS="DELETE"/>
   </FEEPAYMENT>
   <PAYEEHISTORY ENTITYTYPE="PHYSICAL" DATASRCE="PAYEEHISTORY">
      <PAYEEHISTORYSEQNO KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
      <BANKWIZARDINDICATOR DATATYPE="BOOLEAN"/>
   </PAYEEHISTORY>
   <FINDPAYEEHISTORY ENTITYTYPE="PHYSICAL" DATASRCE="PAYEEHISTORY" OUTNAME="PAYEEHISTORY">
      <BANKWIZARDINDICATOR DATATYPE="BOOLEAN"/>
   </FINDPAYEEHISTORY>
   <GETDISBURSEMENTPAYMENTS ENTITYTYPE="LOGICAL" DATASRCE="GETDISBURSEMENTPAYMENTS" OUTNAME="PAYMENTRECORD">
      <PAYMENTTYPE DATATYPE="COMBO" COMBOGROUP="PaymentType"/>
   </GETDISBURSEMENTPAYMENTS>
   <FINDLOANCOMPONENTPAYMENTLIST DATASRCE="FINDLOANCOMPONENTPAYMENTLIST" OUTNAME="LOANCOMPONENTPAYMENT" ENTITYTYPE="LOGICAL">
      <INTERESTONLYELEMENT DATATYPE="LONG"/>
      <CAPITALANDINTERESTELEMENT DATATYPE="LONG"/>
   </FINDLOANCOMPONENTPAYMENTLIST>
   <ROFTOTAL ENTITYTYPE="LOGICAL" DATASRCE="ROFTOTALS">
      <APPLICATIONNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="12"/>
      <ASSOCPAYSEQNUMBER KEYTYPE="PRIMARY" DATATYPE="SHORT"/>
      <LOANCOMPONENTSEQUENCENUMBER DATATYPE="SHORT"/>
      <TOTALRETURNED DATATYPE="LONG"/>
      <INTERESTONLYRETURNED DATATYPE="LONG"/>
      <CAPITALANDINTERESTRETURNED DATATYPE="LONG"/>
   </ROFTOTAL>
   <BATCHPAYMENT ENTITYTYPE="LOGICAL" DATASRCE="BATCHPAYMENTLIST" OUTNAME="PAYMENTRECORD">
      <PAYMENTSEQUENCENUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" SQLNOLOCK="TRUE"/>
   </BATCHPAYMENT>
</PAYMENTPROCESSING>
