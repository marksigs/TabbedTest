Private Sub Views_AddNewMenuItems(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard, ByVal ContextMenu As SnapInLib.ContextMenu, InsertionAllowed As Boolean)

    On Error GoTo ErrTrap_AddNewMenuItems
    ' don't allow create queue if service is not running
    If Selection.SelectionType = siSingleScopeItem Then
        If (IsQueuesFolderKey(Selection.ScopeItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowCreateQueue = False Or m_SnapInData.Computer.GetComputerMQLServiceStatus(GetComputerFromKey(Selection.ScopeItems(1).Key)) <> SERVICESTATUS_STARTED Then
                mnuQueueFolderNewMessageQueue.Grayed = True
            Else
                mnuQueueFolderNewMessageQueue.Grayed = False
            End If
            ContextMenu.AddMenu topmnuQueueFolderNew
        ElseIf (IsQueueEventsKey(Selection.ScopeItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowCreateQueueEvent = False Then
                mnuQueueEventsNewEvent.Grayed = True
            Else
                mnuQueueEventsNewEvent.Grayed = False
            End If
            ContextMenu.AddMenu topmnuQueueEventsNew
        End If
    End If

    Exit Sub
    
' Error Handler for this method
ErrTrap_AddNewMenuItems:
    m_SnapInData.Error.DisplayError "AddNewMenuItems"

End Sub

Private Sub Views_AddTopMenuItems(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard, ByVal ContextMenu As SnapInLib.ContextMenu, InsertionAllowed As Boolean)

    On Error GoTo ErrTrap_AddTopMenuItems

    If Selection.SelectionType = siSingleScopeItem Then
        If (IsEnterpriseServerKey(Selection.ScopeItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowConnectComputer = False Then
                mnuEnterpriseServerConnectComputer.Grayed = True
            Else
                mnuEnterpriseServerConnectComputer.Grayed = False
            End If
            ContextMenu.AddMenu topmnuEnterpriseServerConnect
        ElseIf (IsComputerKey(Selection.ScopeItems(1).Key)) Then
            Dim nServiceStatus As SERVICESTATUS
            nServiceStatus = m_SnapInData.Computer.GetComputerMQLServiceStatus(GetComputerFromKey(Selection.ScopeItems(1).Key))
            Select Case nServiceStatus
                Case SERVICESTATUS_STARTED
                    If m_SnapInData.ConfigData.m_datappgES.m_bAllowStopService = False Then
                        mnuComputerStopService.Grayed = True
                    Else
                        mnuComputerStopService.Grayed = False
                    End If
                    ContextMenu.AddMenu topmnuComputerStopService
                Case SERVICESTATUS_STOPPED
                    If m_SnapInData.ConfigData.m_datappgES.m_bAllowStartService = False Then
                        mnuComputerStartService.Grayed = True
                    Else
                        mnuComputerStartService.Grayed = False
                    End If
                    ContextMenu.AddMenu topmnuComputerStartService
            End Select
        ElseIf (IsQueueKey(Selection.ScopeItems(1).Key)) Then
            Dim nQueueStatus As QUEUESTATUS
            nQueueStatus = m_SnapInData.Queue.GetComputerQueueStatus(Selection.ScopeItems(1).Key)
            Select Case nQueueStatus
                Case QUEUESTATUS_STARTED
                    If m_SnapInData.ConfigData.m_datappgES.m_bAllowStallQueue = False Then
                        mnuQueueStallQueue.Grayed = True
                    Else
                        mnuQueueStallQueue.Grayed = False
                    End If
                    ContextMenu.AddMenu topmnuQueueStallQueue
                Case QUEUESTATUS_STALLED
                    If m_SnapInData.ConfigData.m_datappgES.m_bAllowStartQueue = False Then
                        mnuQueueStallQueue.Enabled = False
                    Else
                        mnuQueueStallQueue.Enabled = True
                    End If
                    ContextMenu.AddMenu topmnuQueueStartQueue
            End Select
        ElseIf (IsStalledComponentsKey(Selection.ScopeItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowStallComponent = False Then
                mnuStalledComponentsStallComponent.Grayed = True
            Else
                mnuStalledComponentsStallComponent.Grayed = False
            End If
            ContextMenu.AddMenu topmnuStalledComponentsStall
        End If
    ElseIf Selection.SelectionType = siSingleListItem Then
        If (IsStalledComponentKey(Selection.ListItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowStartComponent = False Then
                mnuStalledComponentStartComponent.Grayed = True
            Else
                mnuStalledComponentStartComponent.Grayed = False
            End If
            ContextMenu.AddMenu topmnuStalledComponentStart
        End If
    End If

    Exit Sub
    
' Error Handler for this method
ErrTrap_AddTopMenuItems:
    m_SnapInData.Error.DisplayError "AddTopMenuItems"

End Sub

' =============================================================================
' Method:       Views_CreatePropertyPages
' Type:         Event
' Description:  Fired when the user requests properties for the selected
'               items and True has been returned from
'               the Views_QueryPagesFor event. Gives the snap-in the chance
'               to add its property pages to the property sheet being displayed
'               by MMC for the selected items.
'
' Parameters:   View             The View in which the request occurred.
'               Selection        The selected items
'               PropertySheet    PropertySheet object used to add the snap-in's
'                                property pages.
'
' Output:       None
' Notes:
' =============================================================================
'
Private Sub Views_CreatePropertyPages(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard, ByVal PropertySheet As SnapInLib.MMCPropertySheet)

    On Error GoTo ErrTrap_Views_CreatePropertyPages
    
    If Selection.SelectionType = siSingleScopeItem Then
        Select Case GetUICommandFromKey(Selection.ScopeItems(1).Key)
            Case UICOMMANDREQUEST_NEWMESSAGEQUEUE
                Set m_SnapInData.datawpgNewQueue.m_SnapInData = m_SnapInData
                Set m_SnapInData.datawpgNewQueue.m_ScopeItem = Selection.ScopeItems(1)
                m_SnapInData.datawpgNewQueue.m_strComputerName = GetComputerFromKey(Selection.ScopeItems(1).Key)
                PropertySheet.AddWizardPage "wpgQType", m_SnapInData.datawpgNewQueue, False
                PropertySheet.AddWizardPage "wpgQName", m_SnapInData.datawpgNewQueue, False
                PropertySheet.AddWizardPage "wpgQThreads", m_SnapInData.datawpgNewQueue, False
                PropertySheet.AddWizardPage "wpgQConnect", m_SnapInData.datawpgNewQueue, False
                PropertySheet.AddWizardPage "wpgQPolling", m_SnapInData.datawpgNewQueue, False
                PropertySheet.AddWizardPage "wpgQFinish", m_SnapInData.datawpgNewQueue, False
            Case UICOMMANDREQUEST_NEWQUEUEEVENT
                Set m_SnapInData.datawpgNewQEvent.m_SnapInData = m_SnapInData
                Set m_SnapInData.datawpgNewQEvent.m_ScopeItem = Selection.ScopeItems(1)
                m_SnapInData.datawpgNewQEvent.m_strComputerName = GetComputerFromKey(Selection.ScopeItems(1).Key)
                m_SnapInData.datawpgNewQEvent.m_strQueueName = GetQueueFromKey(Selection.ScopeItems(1).Key)
                PropertySheet.AddWizardPage "wpgEType", m_SnapInData.datawpgNewQEvent, False
                PropertySheet.AddWizardPage "wpgEThreads", m_SnapInData.datawpgNewQEvent, False
                PropertySheet.AddWizardPage "wpgEDayTime", m_SnapInData.datawpgNewQEvent, False
                PropertySheet.AddWizardPage "wpgEFinish", m_SnapInData.datawpgNewQEvent, False
            Case UICOMMANDREQUEST_STALLCOMPONENT
                Set m_SnapInData.datawpgStallComp.m_SnapInData = m_SnapInData
                Set m_SnapInData.datawpgStallComp.m_ScopeItem = Selection.ScopeItems(1)
                m_SnapInData.datawpgStallComp.m_strComputerName = GetComputerFromKey(Selection.ScopeItems(1).Key)
                m_SnapInData.datawpgStallComp.m_strQueueName = GetQueueFromKey(Selection.ScopeItems(1).Key)
                PropertySheet.AddWizardPage "wpgStallComp", m_SnapInData.datawpgStallComp, False
            Case Else
                ' properties has been selected
                If (IsEnterpriseServerKey(Selection.ScopeItems(1).Key)) Then
                    m_SnapInData.datappgES.Copy m_SnapInData.ConfigData.m_datappgES
                    Set m_SnapInData.datappgES.m_SnapInData = m_SnapInData
                    Set m_SnapInData.datappgES.m_ScopeItem = Selection.ScopeItems(1)
                    PropertySheet.AddPage "ppgESAccount", "Account", False, False, m_SnapInData.datappgES
                    PropertySheet.AddPage "ppgESPermit", "Permissions", False, False, m_SnapInData.datappgES
                ElseIf (IsComputerKey(Selection.ScopeItems(1).Key)) Then
                    PropertySheet.AddPage "ppgDepServices", "Dependent Services", False, False, Selection.ScopeItems(1).Key
                End If
        End Select
    End If
    
    Exit Sub

' Error Handler for this method
ErrTrap_Views_CreatePropertyPages:
    m_SnapInData.Error.DisplayError "Views_CreatePropertyPages"

End Sub

Private Sub Views_Delete(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard)
    
    On Error GoTo ErrTrap_Views_Delete
    Dim bDeleted As Boolean
    
    ' Delete the scope items from MMC and do the actual delete
    Dim ScopeItem As ScopeItem
    For Each ScopeItem In Selection.ScopeItems
        bDeleted = False
        If (IsComputerKey(ScopeItem.Key)) Then
            bDeleted = m_SnapInData.Computer.DeleteComputer(GetComputerFromKey(ScopeItem.Key))
        ElseIf (IsQueueKey(ScopeItem.Key)) Then
            bDeleted = m_SnapInData.Queue.DeleteQueue(GetComputerFromKey(ScopeItem.Key), GetQueueFromKey(ScopeItem.Key))
        Else
            Debug.Assert 0
        End If
        If bDeleted Then
            ScopeItems.Remove (ScopeItem.Key)
        End If
    Next ScopeItem

    
    ' Delete the list items from MMC and do the actual delete
    Dim ListItem As MMCListItem
    With View.ScopePaneItems.SelectedItem.ResultView.ListView
        For Each ListItem In Selection.ListItems
            bDeleted = False
            If (IsQueueEventKey(ListItem.Key)) Then
                bDeleted = m_SnapInData.QueueEvents.DeleteQueueEvent(ListItem.Key)
            Else
                Debug.Assert 0
            End If
            If bDeleted Then
                .ListItems.Remove (ListItem.Key)
            End If
        Next ListItem
    End With
    
    Exit Sub

' Error Handler for this method
ErrTrap_Views_Delete:
    m_SnapInData.Error.DisplayError "Views_Delete"

End Sub

' =============================================================================
' Method:       RefreshScopeItem
' Type:         Event
' Description:  Fired when the user requests to refresh the view
'               by hitting F5 or selecting "Refresh"
'               from the context menu.
'
' Parameters:   ScopeItem       ScopeItem to be refreshed
'
' Output:       None
' Notes:        Removes all of the child scope items and then iterates through
'               the children and adds them back. If the scope item is
'               currently selected in the scope pane then calls
'               ScopePaneItem.DisplayNewResultView to refresh the result
'               view.
' =============================================================================
'
Public Sub RefreshScopeItem(ByVal ScopeItem As SnapInLib.ScopeItem)

    On Error GoTo ErrTrap_RefreshScopeItem
    
    ' Remove all of the child scope items of the refreshed scope item.
    ScopeItem.RemoveChildren
    
    ' Invoke the ScopeItems_Expand event handler is it knows how to populate the
    ' children of a given ScopeItem
    ScopeItems_Expand ScopeItem

    ' If the refreshed scope item is currently selected in the scope pane then
    ' refresh the result pane contents. This can be done by calling
    ' ScopePaneItem.DisplayNewResultView using the current display string and result
    ' view type.
    If Not CurrentScopePaneItem Is Nothing Then
        If ScopeItem Is CurrentScopePaneItem.ScopeItem Then
            CurrentScopePaneItem.DisplayNewResultView CurrentScopePaneItem.DisplayString, CurrentScopePaneItem.ResultViewType
        End If
    End If
   
    Exit Sub
    
' Error Handler for this method
ErrTrap_RefreshScopeItem:
    m_SnapInData.Error.DisplayError "RefreshScopeItem"

End Sub

