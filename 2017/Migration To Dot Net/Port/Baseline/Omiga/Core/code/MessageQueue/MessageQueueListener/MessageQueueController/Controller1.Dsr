' Method:       Views_QueryPagesFor
' Type:         Event
' Description:  Fired when the user requests properties for the selected
'               items.
'
' Parameters:   View             The View in which the request occurred.
'               Selection        The selected items
'               HasPropertyPages Defaults to False. Set to True if the snap-in
'                                has property pages for the selected items.
'
' Output:       None
' Notes:
' =============================================================================
'
Private Sub Views_QueryPagesFor(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard, HasPropertyPages As Boolean)
    
    On Error GoTo ErrTrap_Views_QueryPagesFor

    If Selection.SelectionType = siSingleScopeItem Then
        If IsEnterpriseServerKey(Selection.ScopeItems(1).Key) Or _
           IsComputerKey(Selection.ScopeItems(1).Key) Or _
           IsQueuesFolderKey(Selection.ScopeItems(1).Key) Or _
           IsQueueKey(Selection.ScopeItems(1).Key) Or _
           IsQueueEventsKey(Selection.ScopeItems(1).Key) Or _
           IsStalledComponentsKey(Selection.ScopeItems(1).Key) Then
           HasPropertyPages = True
           Exit Sub
        End If
    End If

' Error Handler for this method
ErrTrap_Views_QueryPagesFor:
    m_SnapInData.Error.DisplayError "Views_QueryPagesFor"

End Sub

' =============================================================================
' Method:       Views_Refresh
' Type:         Event
' Description:  Fired when the user requests to refresh the view
'               by hitting F5 or selecting "Refresh"
'               from the context menu.
'
' Parameters:   View            A reference to the specific View object
'                               in which the refresh request occurred.
'               Selection       An MMCClipboard object that contains
'                               the currently selected items.
'
' Output:       None
' Notes:        We support refresh for scope items only.
'               Checks selection and calls subroutine RefreshScopeItem to do the work.
' =============================================================================
'
Private Sub Views_Refresh(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard)
    
    On Error GoTo ErrTrap_Views_Refresh
    
    If Selection.SelectionType = siSingleScopeItem Then
        RefreshScopeItem Selection.ScopeItems(1)
    End If
    
    Exit Sub
    
' Error Handler for this method
ErrTrap_Views_Refresh:
    m_SnapInData.Error.DisplayError "Views_Refresh"

End Sub

Private Sub Views_Select(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard, ByVal Selected As Boolean, ByVal ConsoleVerbs As SnapInLib.MMCConsoleVerbs)
    
    On Error GoTo ErrTrap_Views_Select
    
    ConsoleVerbs(siOpen).Enabled = False
    ConsoleVerbs(siCopy).Enabled = False
    ConsoleVerbs(siPaste).Enabled = False
    ConsoleVerbs(siDelete).Enabled = False
    ConsoleVerbs(siProperties).Enabled = False
    ConsoleVerbs(siRename).Enabled = False
    ConsoleVerbs(siRefresh).Enabled = False
    ConsoleVerbs(siPrint).Enabled = False
    ConsoleVerbs(siCut).Enabled = False
    
    If Selection.SelectionType = siSingleScopeItem Then
        If (IsEnterpriseServerKey(Selection.ScopeItems(1).Key)) Then
            ConsoleVerbs(siProperties).Enabled = True
            ConsoleVerbs(siRefresh).Enabled = True
        ElseIf (IsComputerKey(Selection.ScopeItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowDisconnectComputer = True Then
                ConsoleVerbs(siDelete).Enabled = True
            End If
            ConsoleVerbs(siProperties).Enabled = True
            ConsoleVerbs(siRefresh).Enabled = True
        ElseIf (IsQueuesFolderKey(Selection.ScopeItems(1).Key)) Then
            ConsoleVerbs(siRefresh).Enabled = True
        ElseIf (IsQueueKey(Selection.ScopeItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowDeleteQueue = True Then
                ConsoleVerbs(siDelete).Enabled = True
            End If
            ConsoleVerbs(siProperties).Enabled = True
        ElseIf (IsStalledComponentsKey(Selection.ScopeItems(1).Key)) Then
            ConsoleVerbs(siRefresh).Enabled = True
        ElseIf (IsQueueEventsKey(Selection.ScopeItems(1).Key)) Then
            ConsoleVerbs(siRefresh).Enabled = True
        ElseIf (IsNTLogEventsKey(Selection.ScopeItems(1).Key)) Then
            ConsoleVerbs(siRefresh).Enabled = True
        End If
    ElseIf Selection.SelectionType = siSingleListItem Then
        If (IsQueueEventKey(Selection.ListItems(1).Key)) Then
            If m_SnapInData.ConfigData.m_datappgES.m_bAllowDeleteQueueEvent = True Then
                ConsoleVerbs(siDelete).Enabled = True
            End If
        End If
    End If

    Exit Sub

' Error Handler for this method
ErrTrap_Views_Select:
    m_SnapInData.Error.DisplayError "Views_Select"

End Sub

