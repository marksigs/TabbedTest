Private Sub ResultViews_Initialize(ByVal ResultView As SnapInLib.ResultView)

    On Error GoTo ErrTrap_ResultViews_Initialize

    Dim ScopeItem As ScopeItem
    Set ScopeItem = ResultView.ScopePaneItem.ScopeItem
    If IsStalledComponentsKey(ScopeItem.Key) Then
        m_SnapInData.StalledComponents.AddStalledComponentsToView ResultView
    ElseIf IsQueueEventsKey(ScopeItem.Key) Then
        m_SnapInData.QueueEvents.AddQueueEventsToView ResultView
    ElseIf IsNTLogEventsKey(ScopeItem.Key) Then
        m_SnapInData.NTLogEvents.AddNTLogEventsToView ResultView
    End If
    
    Exit Sub

' Error Handler for this method
ErrTrap_ResultViews_Initialize:
