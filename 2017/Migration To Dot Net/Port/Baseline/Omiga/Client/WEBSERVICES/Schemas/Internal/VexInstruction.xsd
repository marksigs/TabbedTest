<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Instruction">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="InstructionRef">
					<xs:annotation>
						<xs:documentation>Lender instruction reference, this is the primary lender reference.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="InstructionRef1">
					<xs:annotation>
						<xs:documentation>Lender instruction reference 1</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="InstructionRef2">
					<xs:annotation>
						<xs:documentation>Lender instruction reference 2</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="InstructionRef3">
					<xs:annotation>
						<xs:documentation>Lender instruction reference 3</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="InstructionDX">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="SourceGUID">
					<xs:annotation>
						<xs:documentation>Unique identifier, supplied to sender by VEx</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="32"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="WhenCreated" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Time date stamp as to when the message was created</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ReadyToSend">
					<xs:annotation>
						<xs:documentation>Flag to show that the message is ready to be sent</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:minInclusive value="0"/>
							<xs:maxInclusive value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="IsSent">
					<xs:annotation>
						<xs:documentation>Flag to show that the message has been sent</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:minInclusive value="0"/>
							<xs:maxInclusive value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="WhenSent" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Time date stamp as to when the message was posted</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="InsertInstruction">
					<xs:annotation>
						<xs:documentation>Instruction Details</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="InstructingSource">
								<xs:annotation>
									<xs:documentation>This specifies what entity type the instruction is sourced from, these can be lender, packager or pmanager</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="32"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Environment" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="500"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="InstructionDate" type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>Date of instruction sent</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="StatusCode">
								<xs:annotation>
									<xs:documentation>Code recognized by VEx as to what action the instruction represents.  So far for this type of instruction this must be populated with NEWINSTR</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="NEWINSTR"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ValuationReqd" type="xs:string">
								<xs:annotation>
									<xs:documentation>Type of valuation required, for example MV for a mortage valuation </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MortgageType">
								<xs:annotation>
									<xs:documentation>Extra information, e.g. for a remortgage</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="25"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="MortgageReqd" type="xs:float">
								<xs:annotation>
									<xs:documentation>Advance value</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PurchasePrice" type="xs:float">
								<xs:annotation>
									<xs:documentation>Price of the property</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ApproxValue" type="xs:float"/>
							<xs:element name="FeePaid" type="xs:float">
								<xs:annotation>
									<xs:documentation>Valuer fee</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Currency">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="20"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="GroundRent" type="xs:float">
