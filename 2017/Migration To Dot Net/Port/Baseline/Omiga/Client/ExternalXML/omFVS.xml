<OM_SCHEMA>
	<ENTITY NAME="FVFILE" READ_REF="FVFILE">
		<ATTRIBUTE NAME="FILEGUID" DATATYPE="GUID" KEYTYPE="PRIMARY"/>
		<ATTRIBUTE NAME="FILEVERSION" DATATYPE="STRING" LENGTH="20"/>
		<ATTRIBUTE NAME="FILENAME" DATATYPE="STRING" LENGTH="39"/>
		<ATTRIBUTE NAME="FILESIZE" DATATYPE="LONG"/>
		<ATTRIBUTE NAME="DATECREATED" DATATYPE="DATE"/>
		<ATTRIBUTE NAME="CREATEDBY" DATATYPE="STRING" LENGTH="25"/>
		<ATTRIBUTE NAME="METADATA" DATATYPE="STRING" LENGTH="100"/>
		<ATTRIBUTE NAME="FILECONTENTSGUID" DATATYPE="GUID"/>
		<ATTRIBUTE NAME="PERMISSIONSGUID" DATATYPE="GUID"/>
		<ENTITY NAME="FVFILECONTENTS" READ_REF="FVFILECONTENTS">
			<ATTRIBUTE NAME="FILECONTENTSGUID" DATATYPE="GUID" KEYTYPE="FOREIGN"/>
			<ATTRIBUTE NAME="STOREAGEMETHOD" DATATYPE="SHORT"/>
			<ATTRIBUTE NAME="FILECONTENTS_TYPE" DATATYPE="STRING" LENGTH="32"/>
			<ATTRIBUTE NAME="FILECONTENTS" DATATYPE="BINARY"/>
			<ATTRIBUTE NAME="COMPRESSIONMETHOD" DATATYPE="STRING" LENGTH="16"/>
			<ATTRIBUTE NAME="DELIVERYTYPE" DATATYPE="SHORT"/>
		</ENTITY>
	</ENTITY>
</OM_SCHEMA>
