<?xml version="1.0" encoding="UTF-8"?>
<!-- EP2_352: Created for remote underwriting task ownership functionality. -->
<!-- EP2_861: do not inherit UNITID, USERID. -->
<!-- EP2_2350: add _LOCKHINT(s) -->
<OM_SCHEMA BOOLEANTRUE="1" BOOLEANFALSE="0">
	<ENTITY NAME="APPLICATION" READ_REF="APPLICATIONFACTFIND" _LOCKHINT="NOLOCK">
		<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="APPLICATIONFACTFIND"/>
		<ENTITY NAME="USERHISTORY" READ_REF="USERHISTORY" _LOCKHINT="NOLOCK" _ORDERBY="(USERHISTORYDATE DESC)">
			<ATTRIBUTE NAME="APPLICATIONNUMBER" ENTITY_REF="USERHISTORY"/>
			<ATTRIBUTE NAME="UNITID" ENTITY_REF="USERHISTORY" KEYTYPE="NULL" NOREADVALUE="TRUE"/>
			<ATTRIBUTE NAME="USERID" ENTITY_REF="USERHISTORY" KEYTYPE="NULL" NOREADVALUE="TRUE"/>
			<ATTRIBUTE NAME="USERHISTORYDATE" ENTITY_REF="USERHISTORY"/>
			<ENTITY NAME="USERROLE" READ_REF="USERROLE" _LOCKHINT="NOLOCK">
				<ATTRIBUTE NAME="UNITID" ENTITY_REF="USERROLE"/>
				<ATTRIBUTE NAME="USERID" ENTITY_REF="USERROLE" NOREADVALUE="TRUE"/>
				<ATTRIBUTE NAME="USERROLEACTIVEFROM" ENTITY_REF="USERROLE"/>
				<ATTRIBUTE NAME="USERROLEACTIVETO" ENTITY_REF="USERROLE"/>
				<ATTRIBUTE NAME="ROLE" ENTITY_REF="USERROLE" GETDEFAULT="65"/> <!-- remote underwriter role -->
			</ENTITY>
		</ENTITY>
	</ENTITY>
</OM_SCHEMA>
