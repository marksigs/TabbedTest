<!--PJO 30/11/2005 MAR741 Changed User ID length to 64 --> 
<OMBATCH_SCHEMA OOSS="VARIANT">
   <BATCH ENTITYTYPE="PHYSICAL" DATASRCE="BATCH" OOSS="VARIANT">
      <BATCHNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="10" OOSS="VARIANT"/>
      <PROGRAMTYPE DATATYPE="SHORT"/>
      <USERID DATATYPE="STRING" LENGTH="64"/>
      <STATUS DATATYPE="SHORT"/>
      <FREQUENCY DATATYPE="SHORT"/>
      <DESCRIPTION DATATYPE="STRING" LENGTH="50"/>
      <EXECUTIONDATETIME DATATYPE="DATETIME"/>
   </BATCH>
   <BATCHSCHEDULE ENTITYTYPE="PHYSICAL" DATASRCE="BATCHSCHEDULE" SQLNOLOCK="TRUE" OOSS="VARIANT">
      <BATCHNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="10" OOSS="VARIANT"/>
      <BATCHRUNNUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" DEFAULT="1"/>
      <STATUS DATATYPE="SHORT"/>
      <CREATIONDATETIME DATATYPE="DATETIME"/>
      <EXECUTIONDATETIME DATATYPE="DATETIME"/>
      <COMPLETIONDATETIME DATATYPE="DATETIME"/>
      <RETRYBATCHINDICATOR DATATYPE="SHORT"/>
      <FAILEDBATCHRUNNUMBER DATATYPE="SHORT"/>
      <ERRORNUMBER DATATYPE="SHORT"/>
      <ERRORSOURCE DATATYPE="STRING" LENGTH="500" OOSS="VARIANT"/>
      <ERRORDESCRIPTION DATATYPE="STRING" LENGTH="255"/>
   </BATCHSCHEDULE>
   <BATCHAUDIT ENTITYTYPE="PHYSICAL" DATASRCE="BATCHAUDIT" OOSS="VARIANT">
      <BATCHNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="10" OOSS="VARIANT"/>
      <BATCHRUNNUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT"/>
      <BATCHAUDITGUID KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="GUID"/>
      <STATUS DATATYPE="SHORT"/>
      <ERRORNUMBER DATATYPE="SHORT"/>
      <ERRORSOURCE DATATYPE="STRING" LENGTH="500" OOSS="VARIANT"/>
      <ERRORDESCRIPTION DATATYPE="STRING" LENGTH="255"/>
      <NUMBEROFRETRIES DATATYPE="SHORT"/>
   </BATCHAUDIT>
   <PAYPROCBATCHPARAMS ENTITYTYPE="PHYSICAL" DATASRCE="PAYPROCBATCHPARAMS" OOSS="VARIANT">
      <BATCHNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="10" OOSS="VARIANT"/>
      <PAYMENTJOBTYPE DATATYPE="SHORT"/>
      <BACS DATATYPE="SHORT"/>
      <CHAPSORTT DATATYPE="SHORT"/>
      <CHEQUE DATATYPE="SHORT"/>
      <INTERNALTRANSFER DATATYPE="SHORT"/>
      <ISSUEDATE DATATYPE="DATETIME"/>
      <APPNUMRANGESTART DATATYPE="STRING" LENGTH="12"/>
      <APPNUMRANGEEND DATATYPE="STRING" LENGTH="12"/>
   </PAYPROCBATCHPARAMS>
   <GETLATESTBATCHSCHEDULE ENTITYTYPE="LOGICAL" DATASRCE="GETLATESTBATCHSCHEDULE" SQLNOLOCK="TRUE" OOSS="VARIANT">
      <BATCHNUMBER ENTITYREF="BATCHSCHEDULE" OOSS="VARIANT"/>
      <BATCHRUNNUMBER ENTITYREF="BATCHSCHEDULE" OOSS="VARIANT"/>
      <STATUS ENTITYREF="BATCHSCHEDULE"/>
      <CREATIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <EXECUTIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <COMPLETIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <TOTALNUMBEROFRECORDS ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <TOTALNUMBEROFFAILURES ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <TOTALNUMBEROFSUCCESSES ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <RETRYBATCHINDICATOR ENTITYREF="BATCHSCHEDULE"/>
      <FAILEDBATCHRUNNUMBER ENTITYREF="BATCHSCHEDULE"/>
   </GETLATESTBATCHSCHEDULE>
   <PRODUCEBATCHSCHEDULEREPORT ENTITYTYPE="LOGICAL" DATASRCE="GETBATCHREPORT" OUTNAME="GETBATCHREPORT" OOSS="VARIANT">
      <BATCHNUMBER ENTITYREF="BATCH"/>
      <PROGRAMTYPE ENTITYREF="BATCH"/>
      <USERID ENTITYREF="BATCH"/>
      <STATUS ENTITYREF="BATCH"/>
      <FREQUENCY ENTITYREF="BATCH"/>
      <DESCRIPTION ENTITYREF="BATCH"/>
      <EXECUTIONDATETIME ENTITYREF="BATCH"/>
      <BATCHRUNNUMBER ENTITYREF="BATCHSCHEDULE"/>
      <STATUS ENTITYREF="BATCHSCHEDULE"/>
      <CREATIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <EXECUTIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <COMPLETIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <TOTALNUMBEROFRECORDS ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <TOTALNUMBEROFFAILURES ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <TOTALNUMBEROFSUCCESSES ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <RETRYBATCHINDICATOR ENTITYREF="BATCHSCHEDULE"/>
      <FAILEDBATCHRUNNUMBER ENTITYREF="BATCHSCHEDULE"/>
      <BATCHAUDITGUID ENTITYREF="BATCHAUDIT"/>
      <STATUS ENTITYREF="BATCHAUDIT"/>
      <ERRORNUMBER ENTITYREF="BATCHAUDIT"/>
      <ERRORSOURCE ENTITYREF="BATCHAUDIT"/>
      <ERRORDESCRIPTION ENTITYREF="BATCHAUDIT"/>
      <NUMBEROFRETRIES ENTITYREF="BATCHAUDIT"/>
      <PAYMENTJOBTYPE ENTITYREF="PAYPROCBATCHPARAMS"/>
      <BACS ENTITYREF="PAYPROCBATCHPARAMS"/>
      <CHAPSORTT ENTITYREF="PAYPROCBATCHPARAMS"/>
      <CHEQUE ENTITYREF="PAYPROCBATCHPARAMS"/>
      <INTERNALTRANSFER ENTITYREF="PAYPROCBATCHPARAMS"/>
      <ISSUEDATE ENTITYREF="PAYPROCBATCHPARAMS"/>
      <APPNUMRANGESTART ENTITYREF="PAYPROCBATCHPARAMS"/>
      <APPNUMRANGEEND ENTITYREF="PAYPROCBATCHPARAMS"/>
   </PRODUCEBATCHSCHEDULEREPORT>
   <GETBATCH ENTITYTYPE="LOGICAL" DATASRCE="GETBATCH" OUTNAME="BATCH" OOSS="VARIANT">
      <BATCHNUMBER ENTITYREF="BATCH" KEYTYPE="PRIMARY"/>
      <PROGRAMTYPE ENTITYREF="BATCH"/>
      <USERID ENTITYREF="BATCH"/>
      <BATCHSTATUS DATATYPE="SHORT"/>
      <FREQUENCY ENTITYREF="BATCH"/>
      <DESCRIPTION ENTITYREF="BATCH"/>
      <BATCHEXECUTIONDATETIME DATATYPE="DATETIME"/>
      <BATCHRUNNUMBER ENTITYREF="BATCHSCHEDULE"/>
      <BATCHSCHEDULESTATUS DATATYPE="SHORT"/>
      <CREATIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <BATCHSCHEDULEEXECUTIONDATETIME DATATYPE="DATETIME"/>
      <COMPLETIONDATETIME ENTITYREF="BATCHSCHEDULE"/>
      <TOTALNUMBEROFRECORDS ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <TOTALNUMBEROFFAILURES ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <TOTALNUMBEROFSUCCESSES ENTITYREF="BATCHSCHEDULEPROGRESS" OOSS="VARIANT"/>
      <RETRYBATCHINDICATOR ENTITYREF="BATCHSCHEDULE"/>
      <FAILEDBATCHRUNNUMBER ENTITYREF="BATCHSCHEDULE"/>
      <PAYMENTJOBTYPE ENTITYREF="PAYPROCBATCHPARAMS"/>
      <BACS ENTITYREF="PAYPROCBATCHPARAMS"/>
      <CHAPSORTT ENTITYREF="PAYPROCBATCHPARAMS"/>
      <CHEQUE ENTITYREF="PAYPROCBATCHPARAMS"/>
      <INTERNALTRANSFER ENTITYREF="PAYPROCBATCHPARAMS"/>
      <ISSUEDATE ENTITYREF="PAYPROCBATCHPARAMS"/>
      <APPNUMRANGESTART ENTITYREF="PAYPROCBATCHPARAMS"/>
      <APPNUMRANGEEND ENTITYREF="PAYPROCBATCHPARAMS"/>
   </GETBATCH>
   <BATCHEXCEPTION ENTITYTYPE="PHYSICAL" DATASRCE="BATCHEXCEPTION" OOSS="VARIANT">
      <BATCHNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="10" OOSS="VARIANT"/>
      <BATCHRUNNUMBER KEYTYPE="PRIMARY" DATATYPE="SHORT"/>
      <BATCHAUDITGUID KEYTYPE="PRIMARY" DATATYPE="GUID"/>
      <ERRORINFORMATION DATATYPE="STRING" LENGTH="30000"/>
      <BATCHSEQNUMBER DATATYPE="SHORT"/>
   </BATCHEXCEPTION>
   <BATCHSCHEDULEPROGRESS ENTITYTYPE="PHYSICAL" DATASRCE="BATCHSCHEDULEPROGRESS" SQLNOLOCK="TRUE" OOSS="VARIANT">
      <BATCHNUMBER KEYTYPE="PRIMARY" DATATYPE="STRING" LENGTH="10" OOSS="VARIANT"/>
      <BATCHRUNNUMBER KEYTYPE="PRIMARY" KEYSOURCE="GENERATED" DATATYPE="SHORT" OOSS="VARIANT"/>
      <TOTALNUMBEROFRECORDS DATATYPE="SHORT" OOSS="VARIANT"/>
      <TOTALNUMBEROFFAILURES DATATYPE="SHORT" OOSS="VARIANT"/>
      <TOTALNUMBEROFSUCCESSES DATATYPE="SHORT" OOSS="VARIANT"/>
   </BATCHSCHEDULEPROGRESS>
</OMBATCH_SCHEMA>
