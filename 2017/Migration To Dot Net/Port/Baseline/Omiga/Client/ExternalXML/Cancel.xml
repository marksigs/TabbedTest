<REQUEST>
   <LOAD TYPE="Cache" SOURCE="APPLICATIONLATESTDETAILS" ALIAS="APPLICATION" SELECT="APPLICATIONNUMBER='[XMLIN.APPLICATIONNUMBER]'"/>
   <LOAD TYPE="Cache" SOURCE="APPLICATION" ALIAS="APPLICATIONFACTFIND"/>
   <LOAD TYPE="Cache" SOURCE="CUSTOMERROLE" SELECT="APPLICATIONNUMBER='[XMLIN.APPLICATIONNUMBER]'"/>
   <LOAD TYPE="Cache" SOURCE="NEWPROPERTY" SELECT="APPLICATIONNUMBER='[APPLICATIONFACTFIND.APPLICATIONNUMBER]' AND APPLICATIONFACTFINDNUMBER=[APPLICATIONFACTFIND.APPLICATIONFACTFINDNUMBER]"/>
   <CUSTOMERNUMBER TYPE="#Constant" NAME="APPLICATIONNUMBER" RULE="CONCATENATE(E,[APPLICATIONFACTFIND.APPLICATIONNUMBER])"/>
   <DOWNLOAD>
      <Omiga4_Client>Yes</Omiga4_Client>
      <Client_Code>Omiga4</Client_Code>
      <Instance>Subs</Instance>
      <Soft_Level>999</Soft_Level>
      <Data_Sent>Cancel</Data_Sent>
      <Combo_Sent>No</Combo_Sent>
   </DOWNLOAD>
   <CUSTOMERDATA>
      <CUSTOMERDATA_TRANSPARENT TYPE="Transparent" SOURCE="CUSTOMERROLE" MIN="1">
         <LOAD TYPE="Cache" SOURCE="CUSTOMER" SELECT="CUSTOMERNUMBER='[CUSTOMERROLE.CUSTOMERNUMBER]'"/>
         <LOAD TYPE="Cache" SOURCE="CUSTOMERVERSION" SELECT="CUSTOMERNUMBER='[CUSTOMERROLE.CUSTOMERNUMBER]' AND CUSTOMERVERSIONNUMBER=[CUSTOMERROLE.CUSTOMERVERSIONNUMBER]"/>
        <Apploan>
            <AN_Application_No SOURCE="#CONSTANT.APPLICATIONNUMBER"/>
            <AN_Client_No SOURCE="CUSTOMERROLE.CUSTOMERNUMBER"/>
            <AN_Client_Seq_No RULE="Counter(APPLOAN)"/>
            <AN_OM4_Client_No SOURCE="CUSTOMERROLE.CUSTOMERNUMBER"/>
         </Apploan>
         <Applicnt TYPE="Table" SOURCE="CUSTOMERVERSION" MIN="1">
            <AT_Client_No SOURCE="CUSTOMERNUMBER"/>
		    <AT_GTX_PARTY_ID SOURCE="CUSTOMER.OTHERSYSTEMCUSTOMERNUMBER"/>
         </Applicnt>
      </CUSTOMERDATA_TRANSPARENT>
   </CUSTOMERDATA>
   <APPLICATIONDATA>
	  <Loanappl/>	
	  <Loanextn TYPE="Table" SOURCE="APPLICATION">
		 <LE_Application_No SOURCE="#CONSTANT.APPLICATIONNUMBER"/>
		 <LE_Orig_Unit_Code>Down</LE_Orig_Unit_Code>
		 <LE_Reas_Can_Dec_Seq>34</LE_Reas_Can_Dec_Seq>
		 <LE_Reas_Canc_Decl>Canc - No reason given/reason </LE_Reas_Canc_Decl>
	  </Loanextn>
   </APPLICATIONDATA>
   <PROPERTYDATA/>
   <LOANDATA/>
   <THRDPRTYDATA/>
   <FINDETSDATA/>
</REQUEST>
