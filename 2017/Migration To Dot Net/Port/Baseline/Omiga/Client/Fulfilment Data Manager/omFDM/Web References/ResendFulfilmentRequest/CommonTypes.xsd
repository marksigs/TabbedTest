  <complexType name="CardPaymentDetailsType">
    <annotation>
      <documentation>Card payment request sent to card vendor</documentation>
    </annotation>
    <sequence>
      <element name="CreditCardDetails" type="common:CreditOrDebitCardDetailsType" />
      <element minOccurs="0" name="MasterAccountNumber" nillable="true">
        <simpleType>
          <restriction base="int">
            <totalDigits value="8" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="MortgageApplicationNumber" nillable="true" type="string" />
      <element name="PaymentMechanism" type="string">
        <annotation>
          <documentation>This value should be set to "CreditCard"</documentation>
        </annotation>
      </element>
      <element name="TransactionMode" type="string">
        <annotation>
          <documentation>P -Production mode. Indicates the transaction is real. 
							N -Sends the transaction to the internal payment simulator. The simulator sends a No value, indicating the transaction was rejected. 
							Y -Sends the transaction to the internal payment simulator. The simulator sends a Yes value, indicating the transaction was accepted. 
							R -Sends the transaction to t	he internal payment simulator. The simulator sends a random Yes or No value. 
			</documentation>
        </annotation>
      </element>
      <element name="TransactionType" nillable="true" type="string">
        <annotation>
          <documentation>This value should be set to "Auth". 
			</documentation>
        </annotation>
      </element>
      <element name="Pipeline" type="string">
        <annotation>
          <documentation>This value should be set to "Payment".
			</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="CardPaymentResultDetailsType">
    <annotation>
      <documentation>Card payment received from card vendor</documentation>
    </annotation>
    <sequence>
      <element name="PaymentResult" type="string">
        <annotation>
          <documentation>Defines the result. 
								"D" = Decline,  "A" = Approve, "E" =  Error
				</documentation>
        </annotation>
      </element>
      <element name="AuthorisationReference" nillable="true">
        <annotation>
          <documentation>Unique ID</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <minLength value="0" />
            <maxLength value="6" />
          </restriction>
        </simpleType>
      </element>
      <element name="TransactionReference" nillable="true">
        <annotation>
          <documentation>Unique ID for the transaction</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <minLength value="0" />
            <maxLength value="36" />
          </restriction>
        </simpleType>
      </element>
      <element name="UpFrontPaymentReason" type="string" />
      <element name="AVSResult" nillable="true" type="string">
        <annotation>
          <documentation>YY - Address and zip code or postal code match 
								YN - Address matches only 
								NY - Zip code or postal code matches 
								NN - Neither address nor zip code or postal code matches; address is not parsable. 
								UU - Address information is unavailable, service is unavailable, or Error; Unknown 
								FF - Issuer does not participate in AVS 
								Blank - No AVS performed
				</documentation>
        </annotation>
      </element>
      <element name="CV2" nillable="true" type="string">
        <annotation>
          <documentation>0 - Card type does not support CVM 
							1 - CVM match 
							2 - CVM did not match issuer value 
							3 - CVM was not processed 
							4 - CVM should be on the card but cardholder indicated otherwise 
							5 - CVM not supported by issuer 
							6 - Not valid 
							7 - No response from server
			</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
