<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:common="http://commontypes.messages.services.dg.ingdirect.com/0.0.1" attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://commontypes.messages.services.dg.ingdirect.com/0.0.1" xmlns="http://www.w3.org/2001/XMLSchema">
  <annotation>
    <documentation>document version = 0.1</documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.6</documentation>
    <documentation>Added RedirectedIndicator element to the 
		BankAccountDetailsType for APP14 - Payment Account Details Screen (S3a) </documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.7</documentation>
    <documentation>Added BuildingInsuranceReinstallmentAmount, PropertyPurchasePrice and PropertyPurchasePrice elements to the 
		PropertyDetailsType for DIS03 - Pay Mortgage Advance Interface </documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.8</documentation>
    <documentation>Added annotations relative to UI0003, and UC APP15 and SRV01</documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.9</documentation>
    <documentation>Modified ClientDetailsType to make some of elments 'nillable' as per requirements in UC APP33</documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.10</documentation>
    <documentation>Added PaymentDayInMonth , OriginatorIdentificationNumber elements to the BankAccountDetailsType 
						as per requirements App14 Screens
		</documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.11</documentation>
    <documentation>Added RegistrationRequestSentDate , RegistrationCompletedDate and MortgageOfferSentDate elements to the SolicitorDetailsType 
						for APP15
		</documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.12</documentation>
    <documentation>ConveyancerResponseType - Sequence min occur set to '0'</documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.13</documentation>
    <documentation>Added FulfilmentResponseType and renamed GUIDType to FulfilmentPackIdType
		</documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.14</documentation>
    <documentation>Changed length parameters for AuthorisationReference and TransactionReference in TakeCardPaymentResponse element. </documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.15</documentation>
    <documentation>CardPaymentDetailsType.MortgageApplicationNumber data type changed from int to String - Ref Defect MAR399  </documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.16</documentation>
    <documentation>Property Details - For few elements the attribute MinOccurs is set to 0 Ref Defect 739 </documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.17</documentation>
    <documentation>GenericMessage import / reference removed - 'Diamond dependency'- .Net problem raised by MSG </documentation>
  </annotation>
  <annotation>
    <documentation>document version = 0.18</documentation>
    <documentation>Modified ClientDetailsType to make all the elements optional for Defect 807</documentation>
  </annotation>
  <complexType name="BankAccountDetailsType">
    <annotation>
      <documentation>Structure for a Payment Account or a Solicitors Bank Account</documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" name="AccountName" nillable="true">
        <annotation>
          <documentation>Name of bank account</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="18" />
          </restriction>
        </simpleType>
      </element>
      <element name="SortCode">
        <annotation>
          <documentation>The 6 digit sort code for the bank account</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="9" />
          </restriction>
        </simpleType>
      </element>
      <element name="AccountNumber">
        <annotation>
          <documentation>The account number for either the solicitor or applicant bank account.</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="13" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="TransposedIndicator">
        <annotation>
          <documentation>Indicates that the bank account number has been altered to comply with BACS/CHAPS file formats.</documentation>
        </annotation>
        <simpleType>
          <restriction base="boolean" />
        </simpleType>
      </element>
      <element minOccurs="0" name="RedirectedIndicator">
        <simpleType>
          <restriction base="boolean" />
        </simpleType>
      </element>
      <element minOccurs="0" name="DDIStatus" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="BankName">
        <simpleType>
          <restriction base="string">
            <maxLength value="18" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="PaymentDayInMonth">
        <simpleType>
          <restriction base="integer" />
        </simpleType>
      </element>
      <element minOccurs="0" name="OriginatorIdentificationNumber">
        <simpleType>
          <restriction base="string" />
        </simpleType>
      </element>
      <element minOccurs="0" name="BankAddress" type="common:StructuredAddressDetailsType" />
    </sequence>
  </complexType>
  <complexType name="StructuredAddressDetailsType">
    <annotation>
      <documentation>Structured address is used for all address capture. For the prospect/customer, structured addresses include Legal, Mailing and Previous addresses. For the mortgage application, structured addresses include the Property Address and Solicitor Address.</documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" name="FlatNameOrNumber" nillable="true">
        <annotation>
          <documentation>Flat name or number of address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="40" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="HouseOrBuildingName" nillable="true">
        <annotation>
          <documentation>House or building name of address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="30" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="HouseOrBuildingNumber" nillable="true">
        <annotation>
          <documentation>House number of address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="6" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="Street" nillable="true">
        <annotation>
          <documentation>Street name of address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="40" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="District" nillable="true">
        <annotation>
          <documentation>District name of address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="40" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="TownOrCity" nillable="true">
        <annotation>
          <documentation>Town or City name of address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="40" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="County" nillable="true">
        <annotation>
          <documentation>County name of address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="40" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="PostCode" type="common:PostCodeType">
        <annotation>
          <documentation>Post code of address. See PostCodeType for formats</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="CountryCode" nillable="true">
        <annotation>
          <documentation>Indicates the country for the address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="3" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="Overriden" nillable="true">
        <annotation>
          <documentation>Indicates that the addresses returned by the PAF search do not include the person's correct address. Allows user to override addrress returned by the PAF search, and enter amended details.</documentation>
        </annotation>
        <simpleType>
          <restriction base="boolean" />
        </simpleType>
      </element>
      <element minOccurs="0" name="AddressType" nillable="true">
        <annotation>
          <documentation>H = Legal address, P = Previous legal, M = Mail address</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
            <enumeration value="H" />
            <enumeration value="P" />
            <enumeration value="M" />
          </restriction>
        </simpleType>
      </element>
    </sequence>
  </complexType>
  <complexType name="PhoneDetailsType">
    <annotation>
      <documentation>Phone number captured on F/E systems is made up of both "AreaCode" and "LocalNumber" elements.</documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" name="AreaCode">
        <simpleType>
          <restriction base="string">
            <pattern value="[0-9]*" />
          </restriction>
        </simpleType>
      </element>
      <element name="LocalNumber">
        <simpleType>
          <restriction base="string">
            <pattern value="[0-9]*" />
          </restriction>
        </simpleType>
      </element>
      <element name="Extension">
        <simpleType>
          <restriction base="string">
            <pattern value="[0-9]*" />
          </restriction>
        </simpleType>
      </element>
      <element name="PhoneType">
        <annotation>
          <documentation>C = Cellphone/Mobile, R = Residential, B = Business, U = Unknown, F = Fax</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
            <enumeration value="C" />
            <enumeration value="R" />
            <enumeration value="B" />
            <enumeration value="U" />
            <enumeration value="F" />
          </restriction>
        </simpleType>
      </element>
    </sequence>
  </complexType>
  <complexType name="CreditOrDebitCardDetailsType">
    <annotation>
      <documentation>This element holds the details of either credit card or debit card details</documentation>
    </annotation>
    <sequence>
      <element name="CardAccountNumber">
        <simpleType>
          <restriction base="string">
            <maxLength value="19" />
          </restriction>
        </simpleType>
      </element>
      <element name="CardExpiryDate" type="string" />
      <element minOccurs="0" name="CardVerificationNumber" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="4" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="CardVerificationIndicator">
        <annotation>
          <documentation>1- CVM value present, and submitted from the Store, 5 - CVM is intentionally not provided.</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <pattern value="1" />
            <pattern value="5" />
          </restriction>
        </simpleType>
      </element>
      <element name="CardType">
        <annotation>
          <documentation>1 - Visa, 2 - MasterCard, 8 - American Express , 9 - Solo, 10 - Switch, 11 - Electron</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <enumeration value="1" />
            <enumeration value="2" />
            <enumeration value="9" />
            <enumeration value="10" />
            <enumeration value="11" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="CardIssueNumber" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="2" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="CardStartDate" nillable="true" type="string" />
      <element name="PaymentAmount" type="decimal" />
      <element name="Address" type="common:StructuredAddressDetailsType" />
    </sequence>
  </complexType>
  <complexType name="ConveyancerAcknowledgementType">
    <annotation>
      <documentation>This is response to all request except polling</documentation>
    </annotation>
    <sequence>
      <element name="SequenceNo" type="common:SequenceNoType">
        <annotation>
          <documentation>IDUK Sequence# sent as part of the request.</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="ConveyancerHeaderType">
    <sequence>
      <element name="Timestamp" type="common:TimeStampType" />
    </sequence>
  </complexType>
  <simpleType name="SequenceNoType">
    <annotation>
      <documentation>IDUK Sequence No. for Conveyancing Request</documentation>
    </annotation>
    <restriction base="string">
      <pattern value="\d{1,10}" />
    </restriction>
  </simpleType>
  <simpleType name="ConveyancerSequenceNoType">
    <annotation>
      <documentation>Conveyancer Sequence No. in the Response</documentation>
    </annotation>
    <restriction base="string">
      <pattern value="\d{1,10}" />
    </restriction>
  </simpleType>
  <simpleType name="SimpleDateType">
    <annotation>
      <documentation>This data type is used to represent simple date of the format YYYY/MM/DD.</documentation>
    </annotation>
    <restriction base="string">
      <pattern value="\d{4}/(0?[1-9]|1[0-2])/(3[01]|[1-2][0-9]|0?[1-9])" />
    </restriction>
  </simpleType>
  <simpleType name="TimeStampType">
    <annotation>
      <documentation>This data type is used to represent time stamp of the format YYYY/MM/DD hh:mm:ss</documentation>
    </annotation>
    <restriction base="string">
      <pattern value="\d{4}/(0?[1-9]|1[0-2])/(3[01]|[1-2][0-9]|0?[1-9]) (0?\d|1[0-9]|2[0-3]):(0?\d|[1-5]\d):(0?\d|[1-5]\d)" />
    </restriction>
  </simpleType>
  <simpleType name="TargetType">
    <restriction base="string">
      <enumeration value="C" />
      <enumeration value="P" />
    </restriction>
  </simpleType>
  <simpleType name="ActionType">
    <restriction base="string">
      <enumeration value="INS" />
      <enumeration value="UPD" />
      <enumeration value="DEL" />
    </restriction>
  </simpleType>
  <simpleType name="GenderType">
    <annotation>
      <documentation>M = Male; F = Female; X = Not disclosed</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="M" />
      <enumeration value="F" />
      <enumeration value="X" />
    </restriction>
  </simpleType>
  <simpleType name="ResidentType">
    <restriction base="string">
      <enumeration value="0" />
      <enumeration value="1" />
      <enumeration value="2" />
    </restriction>
  </simpleType>
  <simpleType name="RelationshipCodeType">
    <restriction base="string">
      <enumeration value="A" />
      <enumeration value="B" />
      <enumeration value="PA" />
    </restriction>
  </simpleType>
  <simpleType name="ClientStatusType">
    <annotation>
      <documentation>This is the customer status indicator. Values 3 and 4 below are legacy from Canada DAWT, and refer to business prospect and customers. 1 = Prospect; 2 = Customer</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="1" />
      <enumeration value="2" />
      <enumeration value="3" />
      <enumeration value="4" />
    </restriction>
  </simpleType>
  <simpleType name="ClientType">
    <restriction base="string">
      <enumeration value="0" />
    </restriction>
  </simpleType>
  <simpleType name="EmailOptionType">
    <restriction base="string">
      <enumeration value="G" />
      <enumeration value="H" />
    </restriction>
  </simpleType>
  <simpleType name="BusinessType">
    <restriction base="string">
      <enumeration value="1" />
      <enumeration value="2" />
      <enumeration value="3" />
      <enumeration value="4" />
    </restriction>
  </simpleType>
  <simpleType name="SpecialNeedType">
    <annotation>
      <documentation>Refers to the Special Needs fulfilment indicator. 0 = No special needs; 1 = Large Print; 2 = Audio fulfilment; 3 = Braille fulfilment</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="0" />
      <enumeration value="L" />
      <enumeration value="A" />
      <enumeration value="B" />
    </restriction>
  </simpleType>
  <simpleType name="StatementType">
    <restriction base="string">
      <enumeration value="1" />
      <enumeration value="2" />
    </restriction>
  </simpleType>
  <simpleType name="FulfilmentPackIdType">
    <annotation>
      <documentation>defines the GUID type to represent the 128 bit windows guid type as 32 hex digit. This field refers to the unique ID assigned to each fulfilment pack.</documentation>
    </annotation>
    <restriction base="string">
      <pattern value="[A-Fa-f0-9]{32}" />
    </restriction>
  </simpleType>
  <simpleType name="FulfilmentResponseType">
    <annotation>
      <documentation>The response is a generic response indicating the outcome of the request. The general values that can be expected are :"Accepted", "Declined", "Error". In case of error/declined response the error message would be set to appropriate message.                             </documentation>
    </annotation>
    <restriction base="string" />
  </simpleType>
  <complexType name="CampaignPersonType">
    <sequence>
      <element name="PersonID" type="integer" />
      <element name="CampaignPersonID" type="integer" />
      <element name="PromotionCode" type="integer" />
      <element name="CampaignReferenceID" type="string" />
      <element name="Primary" type="boolean" />
      <element name="Count" type="integer" />
      <element name="ReferenceRewarded" type="integer" />
      <element name="OldCount" type="integer" />
      <element name="Action" type="common:ActionType" />
    </sequence>
  </complexType>
  <complexType name="ClientDetailsType">
    <sequence>
      <element minOccurs="0" name="CampaignPerson" nillable="true" type="common:CampaignPersonType" />
      <element minOccurs="0" name="Gender" type="common:GenderType">
        <annotation>
          <documentation>Refers to gender of person record - Male, Female or Not Disclosed</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="MiddleName">
        <annotation>
          <documentation>Middle name of person record</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="32" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="DateOfBirth" type="common:SimpleDateType">
        <annotation>
          <documentation>Date of birth of person record.</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="NamePrefix">
        <annotation>
          <documentation>Salutation of person. E.g. Mr, Mrs, etc</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="16" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="ResidentCode" nillable="true" type="common:ResidentType" />
      <element minOccurs="0" name="FirstName">
        <annotation>
          <documentation>First name of person record</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="32" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="LastName">
        <annotation>
          <documentation>Last name (surname) of person record</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="32" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="Email1" nillable="true">
        <annotation>
          <documentation>Email address - format must be as per BR10 in Use Case APP14. E.g. local@sub-domain.top-level-domain </documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="128" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="Number" nillable="true">
        <annotation>
          <documentation>CIF Number of person record</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <maxLength value="10" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="AddressDetail" type="common:StructuredAddressDetailsType" />
      <element minOccurs="0" maxOccurs="unbounded" name="PhoneDetail" type="common:PhoneDetailsType" />
      <element minOccurs="0" name="DontSolicit" type="boolean">
        <annotation>
          <documentation>This is the Marketing Opt In/Out indicator. Refers to the customer choice to receive ING Marketing information.</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="PreferEmail" nillable="true" type="boolean" />
      <element minOccurs="0" name="PreferPhone" nillable="true" type="boolean" />
      <element minOccurs="0" name="PreferMail" nillable="true" type="boolean" />
      <element minOccurs="0" name="ClientStatus" type="common:ClientStatusType" />
      <element minOccurs="0" name="ClientType" nillable="true" type="common:ClientType" />
      <element minOccurs="0" name="ClientSince" nillable="true" type="common:SimpleDateType" />
      <element minOccurs="0" name="PermMsg" nillable="true" type="string" />
      <element minOccurs="0" name="EmailOption" nillable="true" type="common:EmailOptionType" />
      <element minOccurs="0" name="Language" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="BusinessName" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="40" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="BusinessAs" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="40" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="BusinessNumber" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="25" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="BusinessType" nillable="true" type="common:BusinessType" />
      <element minOccurs="0" name="DayOfReg" nillable="true" type="common:SimpleDateType" />
      <element minOccurs="0" name="ProvinceIncorp" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="2" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="SoleProprietorFirstName" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="32" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="SoleProprietorLastName" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="32" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="SoleProprietorInitial" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="4" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="IncorpType" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="SoleProprietorNamePrefix" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="16" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="IncorpFlag" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="SoleProprietorDateOfBirth" nillable="true" type="common:SimpleDateType" />
      <element minOccurs="0" name="SoleProprietorTaxID" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="12" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="SpecialNeed" type="common:SpecialNeedType" />
      <element minOccurs="0" name="R85Required" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="NationalInsuranceNumber" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="9" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="StatementType" nillable="true" type="common:StatementType" />
      <element minOccurs="0" name="EmailValid" nillable="true">
        <simpleType>
          <restriction base="string">
            <maxLength value="1" />
          </restriction>
