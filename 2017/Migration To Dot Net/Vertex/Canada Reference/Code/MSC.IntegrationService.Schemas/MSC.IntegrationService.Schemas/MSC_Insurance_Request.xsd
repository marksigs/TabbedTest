					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="A"/>
						<xs:enumeration value="U"/>
						<xs:enumeration value="C"/>
						<xs:enumeration value="L"/>
						<xs:enumeration value="P"/>
						<xs:enumeration value="I"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="REFERRALREQUESTED" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates if referral has been requested</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="INSURERCERTIFICATENUMBER" use="optional">
				<xs:annotation>
					<xs:documentation>This is insurance reference number or certificate number. INSURERCERTIFICATENUMBER is "" for initial (A) request </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="REQUESTDATE" type="xs:date" use="required">
				<xs:annotation>
					<xs:documentation>Request date </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MORGAGEINSURERPRODUCT" type="xs:string">
				<xs:annotation>
					<xs:documentation>Mortgage Insurer Product </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="USER" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>User </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PASSWORD" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Password </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CUSTID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation> Customer ID  </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CERTIFAXREQUESTED" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Indicates if CerfiFax has been requested</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="INSURERAPPID" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>This is the application ID from mortgage insurance system. 
					CMHC: It is optional. We set it to the certificate number for CMHC
					GE: It is mandatory. It is different from the certificate number in GE. 
					AIG: It is optional.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FIRSTTIMESUBMISSION" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates if this is first time submission. This attribute is only used by Omiga</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="VERSION" type="xs:string" use="optional" fixed="1.0">
				<xs:annotation>
					<xs:documentation>Each version defines a slightly different data format. This is the version number of the document. We define the first version number as "1.0".</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="UNEARNEDINCOME">
		<xs:complexType>
			<xs:attribute name="UNEARNEDINCOMETYPE">
				<xs:annotation>
					<xs:documentation>Unearned Income Type (UnEarnedIncomeDescription) value:1-Pension 2-Investments 3-Support/Alimony 4-Car Allowance, 5-Other Rental Property(not used in MorWEB), 6-Child Credit Tax Source(not used in MorWEB) 99-Other</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
						<xs:enumeration value="4"/>
						<xs:enumeration value="5"/>
						<xs:enumeration value="6"/>
						<xs:enumeration value="99"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="UNEARNEDINCOMEAMOUNT" use="required">
				<xs:annotation>
					<xs:documentation>Unearned income amount.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="9"/>
						<xs:fractionDigits value="2" fixed="true"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PAYMENTFREQUENCY">
				<xs:annotation>
					<xs:documentation>The income payment frequency  value:1-Annually 2-Semi-annual 4-Quarterly 12-Monthly 24-Semi-monthly 26-Bi-weekly 52-Weekly</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="4"/>
						<xs:enumeration value="12"/>
						<xs:enumeration value="24"/>
						<xs:enumeration value="26"/>
						<xs:enumeration value="52"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="UNITCONTACTDETAILS">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CONTACTDETAILS"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="UWRITER">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CONTACTDETAILS"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
